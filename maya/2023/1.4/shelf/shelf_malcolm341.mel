        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Weld Verts to Closest v1.3\nglobal proc m341_weldVertsToClosest()\n{\n  \t//Error if the selection mask is in object mode bacause it will take too long\n    string $xformCheck[] = `ls -sl -exactType transform`;\n    int $xformCheckSize = `size $xformCheck`;\n    if ($xformCheckSize > 0)\n    {\n       error \"Please select one or more components\";\n    }\n    \n\t//Query what component mode the user is in\n\tglobal int $m341_weldTC_component_Face;\n\t$m341_weldTC_component_Face = `selectType -q -facet`;\n\t\n\tglobal int $m341_weldTC_component_Shell;\n\t$m341_weldTC_component_Shell = `selectType -q -meshUVShell`;\n\t\n\tglobal int $m341_weldTC_component_Vert;\n\t$m341_weldTC_component_Vert = `selectType -q -vertex`;\n\t\n\tglobal int $m341_weldTC_component_Edge;\n\t$m341_weldTC_component_Edge = `selectType -q -edge`;\n\t\n\tglobal int $m341_weldTC_component_UV;\n\t$m341_weldTC_component_UV = `selectType -q -polymeshUV`;\n\t\n\tglobal int $m341_weldTC_component_Multi;\n\t$m341_weldTC_component_Multi = `selectType -q -meshComponents`;\n    \n\n\t//Convert selection to verts\n\tConvertSelectionToVertices;\n\n\t//Store first vert\n\tstring $vertToMove[] = `ls -sl -fl`;\n\tint $vtmCounter = 0;\n        \n\t//Store the number of verts selected\n\t$vertsToWeldSize = `size $vertToMove`;\n\n    \n    //Loop through each vert and run the welding script on it\n   \tfor ($item in $vertToMove)\n\t{\n\t\t//Select the first vert in the list\n\t\tselect $item;\n\t\t\n\t\t//Find the world space position of the vert\n\t\tfloat $vertToMovePOS[] = `pointPosition -w $vertToMove[$vtmCounter]`;\n\t\t\n\t\t//Grow selection to get neighbour verts and store them\n\t\tPolySelectTraverse 1;\n\t\tstring $vertNeighbours[] = `ls -sl -fl`;\n        \n\t\t//Convert to object mode and select all verts\n\t\ttoggleSelMode;\n\t\ttoggleSelMode;\n\t\tselectMode -object;\n\t\tDeleteHistory;\n\t\tConvertSelectionToVertices;\n\t\tpolyConvertToShellBorder;\n\t\t\n\t\t//Add previous neighbour verts\n\t\tselect -add $vertNeighbours;\n\t\t\n\t\t//Store new verts\n\t\tselect -deselect $vertToMove[$vtmCounter];\n\t\tstring $closeVerts[] = `ls -sl -fl`;\n\t\t\n\t\t//Find out how many verts in close verts selection\n\t\tint $closeVertsSize = `size $closeVerts`;\n\t\t\n\t\tfloat $closestDistance = 10000;\n\t\tint $counter = 0;\n\n\t\t\n\t    //Loop through and find the closest distance\n\t    for ($item in $closeVerts)\n\t    {\n            select $item;\n\t        float $coseVertPOS[] = `pointPosition -w`;\n           \t\n\t\t    //Find the difference between the two positions\n\t\t    float $xValue = $coseVertPOS[0] - $vertToMovePOS[0];\n\t\t    float $yValue = $coseVertPOS[1] - $vertToMovePOS[1];\n\t\t    float $zValue = $coseVertPOS[2] - $vertToMovePOS[2];\n\t\t    //vector $difference = <<$xValue, $yValue, $zValue>>;\n\t\t    //Find the distance between the two points\n\t\t    //float $distance = `mag $difference`;\n\n\t\t    //Power\n\t\t    $xValue = $xValue * $xValue;\n\t\t    $yValue = $yValue * $yValue;\n\t\t    $zValue = $zValue * $zValue;\n\t\t    float $xyzValue = $xValue + $yValue + $zValue;\n\t\t    \n\t\t    //Mag written out by hand\n\t\t    float $distance = `sqrt $xyzValue`;\n\t\t\t\t\t\n    \t    //If the distance between the verts is lower than the distance between the previous vert\n\t\t    if ($distance < $closestDistance)\n\t\t    {\n\t\t        //Store the closest distance so far\n\t    \t    $closestDistance = $distance;\n    \t\t    //Store the closest position so far\n\t\t        $closestPosition = $coseVertPOS;\n       \t\t    //Store the closest position so far\n       \t\t    global string $m341_closestVertWeld[]; \n\t\t        $m341_closestVertWeld = `ls -sl`;\n\t\t    \n\t\t        //Move vert to closest vert it found\n                select $vertToMove[$vtmCounter];\n        \t    move -ws $closestPosition[0] $closestPosition[1] $closestPosition[2];\n    \t    }\n\t\t    $counter = $counter + 1;\n        }\n        $vtmCounter = $vtmCounter + 1;\n        select -clear;\n    }\n\t\n    //Convert to object mode merge verts and delete history\n    toggleSelMode;\n    toggleSelMode;\n    selectMode -object;\n    polyMergeVertex -am 1 -ch 0 -distance 0.01;\n    toggleSelMode;\n    toggleSelMode;\n    selectMode -object;\n    DeleteHistory;\n   \n\t//Bring back original component mode user was in\t\t\n\tglobal int $m341_weldTC_component_Face;    \n\tif ($m341_weldTC_component_Face > 0)\n\t{\n\t\tSelectFacetMask;\n\t}\n\n\tglobal int $m341_weldTC_component_Shell;\n\tif ($m341_weldTC_component_Shell > 0)\n\t{\n\t\tSelectMeshUVShell;\n\t}\n\n\tglobal int $m341_weldTC_component_Vert;\t\n\tif ($m341_weldTC_component_Vert > 0)\n\t{\n\t\tSelectVertexMask;\n\t}\n\n\tglobal int $m341_weldTC_component_Edge;\n\tif ($m341_weldTC_component_Edge > 0)\n\t{\n\t\tSelectEdgeMask;\n\t}\n\n\tglobal int $m341_weldTC_component_UV;\n\tif ($m341_weldTC_component_UV > 0)\n\t{\n\t\tSelectUVMask;\n\t}\n\n\tglobal int $m341_weldTC_component_Multi;\t\n\tif ($m341_weldTC_component_Multi > 0)\n\t{\n\t\tSelectMultiComponentMask;\n\t}\n\n\tMoveTool;\n    select -clear;\n\tprint \"Weld to closest complete\\n\";\n}\nm341_weldVertsToClosest();" 
