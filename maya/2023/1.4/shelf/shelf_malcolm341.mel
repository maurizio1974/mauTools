        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\nglobal string $m341_OpenFasterVersion;\n$m341_OpenFasterVersion = \"slowPlugins v1.3\";\n\n//*******************************************************************************************************\n//* global procs\n//*******************************************************************************************************\n//***********************************************************************************************************************************************************\n//Help\nglobal proc m341_helpOpenFaster()\n{\n\tglobal string $m341_openFaster_HelpWindow;\n\n\tif (`window -exists $m341_openFaster_HelpWindow`)\n\t{\n\t\tglobal string $m341_openFaster_HelpWindow;\n\t\tdeleteUI $m341_openFaster_HelpWindow;\n\t}\n\t\n\t$m341_openFaster_HelpWindow = `window -title \"Slow Plugins Help\" -leftEdge 900 -topEdge 115 -toolbox true -sizeable false -resizeToFitChildren false -width 800 -height 260`;\n\t\t\n\tpaneLayout -configuration \"horizontal2\";\n\tscrollField -text \"MASH ABC BIFROST XGEN ARNOLD CHECKBOXES\\n-Turn off checkboxes to unload plug-ins you don't use which will speed up Maya start time a lot. Turn on a checkbox to load that particular plug-in if you need it.\\n\\n\\nECHO CHECKBOX\\n-Turn off Echo checkbox to disable script editor echoing all commands, this will also speed up Maya start time and make Maya run faster in general.\\n\\n\\nPLUGIN MANAGER\\n-Opens the Maya plug-in manager.\\n\\n\\nYOUTUBE VIDEO HERE\\nhttps://youtu.be/xdf9JgMQPkY?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\" \n\t-wordWrap true\n\t-enableBackground true\n\t-backgroundColor 0.23 0.23 0.23;\n\t\n\tshowWindow $m341_openFaster_HelpWindow;\n}\n\n\n//*******************************************************************************************************\n//MASH Toggle\nglobal proc toggleMASH()\n{\n\tint $MASH_CheckMarkValue = `checkBox -query -value MASH_CheckBox`;\n\tif ($MASH_CheckMarkValue == 1)\n\t{\n\t\tloadPlugin \"MASH.mll\";\n\t\tpluginInfo -edit -autoload true \"MASH.mll\";\n\t\tif (`window -exists pluginManagerWindow`)\n\t\t{\n\t\t\tupdatePluginDirectories;\n\t\t}\n\t\tprint \"MASH Plugin loaded\";\n\t}\n\t\n\tif ($MASH_CheckMarkValue == 0)\n\t{\n\t\tunloadPlugin \"MASH.mll\";\n\t\tpluginInfo -edit -autoload false \"MASH.mll\";\n\t\tif (`window -exists pluginManagerWindow`)\n\t\t{\n\t\t\tupdatePluginDirectories;\n\t\t}\n\t\tprint \"MASH Plugin unloaded\";\n\t}\n}\n\n//*******************************************************************************************************\n//ABC Toggle\nglobal proc toggleABC()\n{\n\tstring $maCurrentMayaVersionMac = `about -mac`;\n\tint $ABC_CheckMarkValue = `checkBox -query -value ABC_CheckBox`;\n\tif ($ABC_CheckMarkValue == 1)\n\t{\n\t\tif($maCurrentMayaVersionMac == 1)\n\t\t{\n\t\t\t//Load plugins on Mac\n\t\t\tloadPlugin \"AbcBullet.bundle\";\n\t\t\tpluginInfo -edit -autoload true \"AbcBullet.bundle\";\n\t\t\tloadPlugin \"AbcExport.bundle\";\n\t\t\tpluginInfo -edit -autoload true \"AbcExport.bundle\";\n\t\t\tloadPlugin \"AbcImport.bundle\";\n\t\t\tpluginInfo -edit -autoload true \"AbcImport.bundle\";\n\t\t\tif (`window -exists pluginManagerWindow`)\n\t\t\t{\n\t\t\t\tupdatePluginDirectories;\n\t\t\t}\n\t\t\tprint \"ABC Plugins loaded\";\n\t\t}\t\n\t\telse\n\t\t{\n\t\t\t//Load plugins on PC\n\t\t\tloadPlugin \"AbcBullet.mll\";\n\t\t\tpluginInfo -edit -autoload true \"AbcBullet.mll\";\n\t\t\tloadPlugin \"AbcExport.mll\";\n\t\t\tpluginInfo -edit -autoload true \"AbcExport.mll\";\n\t\t\tloadPlugin \"AbcImport.mll\";\n\t\t\tpluginInfo -edit -autoload true \"AbcImport.mll\";\n\t\t\tif (`window -exists pluginManagerWindow`)\n\t\t\t{\n\t\t\t\tupdatePluginDirectories;\n\t\t\t}\n\t\t\tprint \"ABC Plugins loaded\";\n\t\t}\t\n\t}\n\t\n\tif ($ABC_CheckMarkValue == 0)\n\t{\n\t\tif($maCurrentMayaVersionMac == 1)\n\t\t{\n\t\t\t//Unload plugins on Mac\n\t\t\tunloadPlugin \"AbcBullet.mll\";\n\t\t\tpluginInfo -edit -autoload false \"AbcBullet.bundle\";\n\t\t\tunloadPlugin \"AbcExport.mll\";\n\t\t\tpluginInfo -edit -autoload false \"AbcExport.bundle\";\n\t\t\tunloadPlugin \"AbcImport.mll\";\n\t\t\tpluginInfo -edit -autoload false \"AbcImport.bundle\";\n\t\t\tif (`window -exists pluginManagerWindow`)\n\t\t\t{\n\t\t\t\tupdatePluginDirectories;\n\t\t\t}\n\t\t\tprint \"ABC Plugins unloaded\";\n\t\t}\n\t\telse\n\t\t{\n\t\t\t//Unload plugins on PC\n\t\t\tunloadPlugin \"AbcBullet.mll\";\n\t\t\tpluginInfo -edit -autoload false \"AbcBullet.mll\";\n\t\t\tunloadPlugin \"AbcExport.mll\";\n\t\t\tpluginInfo -edit -autoload false \"AbcExport.mll\";\n\t\t\tunloadPlugin \"AbcImport.mll\";\n\t\t\tpluginInfo -edit -autoload false \"AbcImport.mll\";\n\t\t\tif (`window -exists pluginManagerWindow`)\n\t\t\t{\n\t\t\t\tupdatePluginDirectories;\n\t\t\t}\n\t\t\tprint \"ABC Plugins unloaded\";\n\t\t}\n\t}\n}\n\n//*******************************************************************************************************\n//Bifrost Toggle\nglobal proc toggleBifrost()\n{\n\tint $Bifrost_CheckMarkValue = `checkBox -query -value Bifrost_CheckBox`;\n\tif ($Bifrost_CheckMarkValue == 1)\n\t{\n\t\tloadPlugin \"bifrostGraph.mll\";\n\t\tpluginInfo -edit -autoload true \"bifrostGraph.mll\";\n\t\tloadPlugin \"bifrostshellnode.mll\";\n\t\tpluginInfo -edit -autoload true \"bifrostshellnode.mll\";\n\t\tloadPlugin \"bifrostvisplugin.mll\";\n\t\tpluginInfo -edit -autoload true \"bifrostvisplugin.mll\";\n\t\tif (`window -exists pluginManagerWindow`)\n\t\t{\n\t\t\tupdatePluginDirectories;\n\t\t}\n\t\tprint \"Bifrost Plugins loaded\";\n\t}\n\t\n\tif ($Bifrost_CheckMarkValue == 0)\n\t{\n\t\tunloadPlugin \"bifrostGraph.mll\";\n\t\tpluginInfo -edit -autoload false \"bifrostGraph.mll\";\n\t\tunloadPlugin \"bifrostshellnode.mll\";\n\t\tpluginInfo -edit -autoload false \"bifrostshellnode.mll\";\n\t\tunloadPlugin \"bifrostvisplugin.mll\";\n\t\tpluginInfo -edit -autoload false \"bifrostvisplugin.mll\";\n\t\tif (`window -exists pluginManagerWindow`)\n\t\t{\n\t\t\tupdatePluginDirectories;\n\t\t}\n\t\tprint \"Bifrost Plugins unloaded\";\n\t}\n}\n\n//*******************************************************************************************************\n//XGen Toggle\nglobal proc toggleXGen()\n{\n\tint $XGen_CheckMarkValue = `checkBox -query -value XGen_CheckBox`;\n\tif ($XGen_CheckMarkValue == 1)\n\t{\n\t\tloadPlugin \"xgenToolkit.mll\";\n\t\tpluginInfo -edit -autoload true \"xgenToolkit.mll\";\n\t\tif (`window -exists pluginManagerWindow`)\n\t\t{\n\t\t\tupdatePluginDirectories;\n\t\t}\n\t\tprint \"XGen Plugin loaded\";\n\t}\n\t\n\tif ($XGen_CheckMarkValue == 0)\n\t{\n\t\tunloadPlugin \"xgenToolkit.mll\";\n\t\tpluginInfo -edit -autoload false \"xgenToolkit.mll\";\n\t\tif (`window -exists pluginManagerWindow`)\n\t\t{\n\t\t\tupdatePluginDirectories;\n\t\t}\n\t\tprint \"XGen Plugin unloaded\";\n\t}\n}\n\n//*******************************************************************************************************\n//Arnold Toggle\nglobal proc toggleArnold()\n{\n\tint $Arnold_CheckMarkValue = `checkBox -query -value Arnold_CheckBox`;\n\tif ($Arnold_CheckMarkValue == 1)\n\t{\n\t\tloadPlugin \"mtoa.mll\";\n\t\tpluginInfo -edit -autoload true \"mtoa.mll\";\n\t\tif (`window -exists pluginManagerWindow`)\n\t\t{\n\t\t\tupdatePluginDirectories;\n\t\t}\n\t\tprint \"Arnold Plugin loaded\";\n\t}\n\t\n\tif ($Arnold_CheckMarkValue == 0)\n\t{\n\t\tunloadPlugin \"mtoa.mll\";\n\t\tpluginInfo -edit -autoload false \"mtoa.mll\";\n\t\tif (`window -exists pluginManagerWindow`)\n\t\t{\n\t\t\tupdatePluginDirectories;\n\t\t}\n\t\tprint \"Arnold Plugin unloaded\";\n\t}\n}\n\n//*******************************************************************************************************\n//Echo All Toggle\nglobal proc toggleEchoAll()\n{\n\tint $EchoAll_CheckMarkValue = `checkBox -query -value EchoAll_CheckBox`;\n\tif ($EchoAll_CheckMarkValue == 1)\n\t{\n\t\tif (`window -exists scriptEditorPanel1Window`)\n\t\t{\n\t\t\tcommandEcho -state 1;\t\t\n\t\t\tdeleteUI scriptEditorPanel1Window;\n\t\t\tScriptEditor;\n\t\t}\n\t\telse\n\t\t{\n\t\t\tScriptEditor;\n\t\t\tcommandEcho -state 1;\n\t\t\tdeleteUI scriptEditorPanel1Window;\n\t\t}\n\t\tprint \"Echo all on\";\n\t}\n\t\n\tif ($EchoAll_CheckMarkValue == 0)\n\t{\n\t\tif (`window -exists scriptEditorPanel1Window`)\n\t\t{\n\t\t\tcommandEcho -state 0;\t\t\n\t\t\tdeleteUI scriptEditorPanel1Window;\n\t\t\tScriptEditor;\n\t\t}\n\t\telse\n\t\t{\n\t\t\tScriptEditor;\n\t\t\tcommandEcho -state 0;\n\t\t\tdeleteUI scriptEditorPanel1Window;\n\t\t}\n\t\tprint \"Echo all off\";\n\t}\n}\n\n//*******************************************************************************************************\n//Plugin Manager\nglobal proc m341_openPluginManager() \n{\n\tPluginManager;\n}\n\n//*******************************************************************************************************\n//* window and buttons\n//*******************************************************************************************************\nif (`window -exists m341_OpenFasterUIName`)\n{\n\tdeleteUI m341_OpenFasterUIName;\n}\n\n//Global ints to make checkboxes stay in sync if toolbox is reopended\nglobal int $m341_MASHPluginState;\n$m341_MASHPluginState = 0;\nint $isMASHLoaded = `pluginInfo -q -loaded \"MASH.mll\"`;\nif ($isMASHLoaded == 1)\n{\n\tglobal int $m341_MASHPluginState;\n\t$m341_MASHPluginState = 1;\n}\n\nstring $maCurrentMayaVersionMac = `about -mac`;\nglobal int $m341_ABCPluginState;\n$m341_ABCPluginState = 0;\nglobal int $m341_ABCPluginStateMac;\n$m341_ABCPluginStateMac = 0;\nint $isABCLoaded = `pluginInfo -q -loaded \"AbcExport.mll\"`;\nint $isABCLoadedMac = `pluginInfo -q -loaded \"AbcExport.bundle\"`;\n\nif($maCurrentMayaVersionMac == 1)\n{\n\tif ($isABCLoaded == 1)\n\t{\n\t\tglobal int $m341_ABCPluginStateMac;\n\t\t$m341_ABCPluginStateMac = 1;\n\t\t//Fix for Maya 2022 where abcBullet is not loaded by default\n\t\tloadPlugin \"AbcBullet.bundle\";\n\t\tpluginInfo -edit -autoload true \"AbcBullet.bundle\";\n\t\tif (`window -exists pluginManagerWindow`)\n\t\t{\n\t\t\tupdatePluginDirectories;\n\t\t}\n\t}\n}\nelse\n{\n\tif ($isABCLoaded == 1)\n\t{\n\t\tglobal int $m341_ABCPluginState;\n\t\t$m341_ABCPluginState = 1;\n\t\t//Fix for Maya 2022 where abcBullet is not loaded by default\n\t\tloadPlugin \"AbcBullet.mll\";\n\t\tpluginInfo -edit -autoload true \"AbcBullet.mll\";\n\t\tif (`window -exists pluginManagerWindow`)\n\t\t{\n\t\t\tupdatePluginDirectories;\n\t\t}\n\t}\n}\n\nglobal int $m341_BifrostPluginState;\n$m341_BifrostPluginState = 0;\nint $isBifrostLoaded = `pluginInfo -q -loaded \"bifrostGraph.mll\"`;\nif ($isBifrostLoaded == 1)\n{\n\tglobal int $m341_BifrostPluginState;\n\t$m341_BifrostPluginState = 1;\n}\n\nglobal int $m341_XGenPluginState;\n$m341_XGenPluginState = 0;\nint $isXGenLoaded = `pluginInfo -q -loaded \"xgenToolkit.mll\"`;\nif ($isXGenLoaded == 1)\n{\n\tglobal int $m341_XGenPluginState;\n\t$m341_XGenPluginState = 1;\n}\n\nglobal int $m341_ArnoldPluginState;\n$m341_ArnoldPluginState = 0;\nint $isArnoldLoaded = `pluginInfo -q -loaded \"mtoa.mll\"`;\nif ($isArnoldLoaded == 1)\n{\n\tglobal int $m341_ArnoldPluginState;\n\t$m341_ArnoldPluginState = 1;\n}\n\n//Load script editor if it's closed at start up so UI checkbox query works\nif (`window -exists scriptEditorPanel1Window`)\n{\n\t//Do nothing\n}\nelse\n{\n\tScriptEditor;\n\tdeleteUI scriptEditorPanel1Window;\n}\n\nglobal int $m341_EchoAllPluginState;\n$m341_EchoAllPluginState = 0;\nint $isEchoAllLoaded = `commandEcho -q -state`;\nif ($isEchoAllLoaded == 1)\n{\n\tglobal int $m341_EchoAllPluginState;\n\t$m341_EchoAllPluginState = 1;\n}\n\nwindow -title $m341_OpenFasterVersion -toolbox true -sizeable true -resizeToFitChildren true m341_OpenFasterUIName;\ncolumnLayout -adjustableColumn true;\ntext -label \"by Malcolm341\";\n\n\tsetParent ..;\n\trowLayout -numberOfColumns 1 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 50 -columnWidth 2 30;\n\ticonTextButton -style \"textOnly\"\n\t\t-labelOffset 30\n\t\t-label \"click here for help\"\n\t\t-backgroundColor 0.51 0.51 0.51\n\t\t-width 117\n\t\t-height 15\n\t\t-command \"m341_helpOpenFaster;\";\n\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\tcheckBox -label \"MASH\" -width 66 -value $m341_MASHPluginState -changeCommand \"toggleMASH();\" MASH_CheckBox;\n\tcheckBox -label \"ABC\" -value $m341_ABCPluginState -changeCommand \"toggleABC();\" ABC_CheckBox;\n\tsetParent ..;\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\tcheckBox -label \"Bifrost\" -width 66 -value $m341_BifrostPluginState -changeCommand \"toggleBifrost();\" Bifrost_CheckBox;\n\tcheckBox -label \"XGen\" -value $m341_XGenPluginState -changeCommand \"toggleXGen();\" XGen_CheckBox;\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\tcheckBox -label \"Arnold\" -width 66 -value $m341_ArnoldPluginState -changeCommand \"toggleArnold();\" Arnold_CheckBox;\n\tcheckBox -label \"Echo\" -value $m341_EchoAllPluginState -changeCommand \"toggleEchoAll();\" EchoAll_CheckBox;\n\t\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\ticonTextButton -style \"textOnly\"\n\t\t-backgroundColor 0.51 0.51 0.51\n\t\t-width 117\n\t\t-label \"plugin manager\"\n\t\t-command \"m341_openPluginManager;\"\n\t\t-commandRepeatable 1;\n\t\t\t\n\t\nshowWindow;\nprint \"slowPlugins toolbox loaded\";\n\n\n\n\n\n\n" 
