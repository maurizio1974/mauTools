        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//vertSnapper v1.3\n\n//***********************************************************************************************************************************************************\n//* global variables need to be defined inside the window code when used in a button\n//***********************************************************************************************************************************************************\n\n\n//***********************************************************************************************************************************************************\n//* global procs\n//***********************************************************************************************************************************************************\n//***********************************************************************************************************************************************************\n//Help\nglobal proc helpMaVertSnapper()\n{\n\tglobal string $m341_vertSnapper_HelpWindow;\n\t\n\tif (`window -exists $m341_vertSnapper_HelpWindow`)\n\t{\n\t\tglobal string $m341_vertSnapper_HelpWindow;\n\t\tdeleteUI $m341_vertSnapper_HelpWindow;\n\t}\n\t\n\t$m341_vertSnapper_HelpWindow = `window -title \"vertSnapper Help\" -leftEdge 900 -topEdge 115 -toolbox true -sizeable false -resizeToFitChildren false -width 800 -height 225`;\n\t\t\n\tpaneLayout -configuration \"horizontal2\";\n\tscrollField -text \"Select an object to snap to and click the load button.\\n\\nSelect a different object or some verts and click the snap verts button to snap those verts to the closest vert on the currently loaded object.\\n\\nThe current object to snap to is displayed in the text field.\\n\\nChange the snap radius to adjust how far the selected verts will try to snap to the currently loaded object.\\n\\nTurn off the snap radius tickbox if you want to snap the verts using an infinite radius. This can be handy to quickly snap a couple remaining verts without needing to fiddle with the snap radius to get the right number.\\n\\nYoutube video here\\nhttps://youtu.be/_TTDyLZ-nXU?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\"\n\t-wordWrap true\n\t-enableBackground true\n\t-backgroundColor 0.23 0.23 0.23;\n\t\n\tshowWindow $m341_vertSnapper_HelpWindow;\n}\n\n//***********************************************************************************************************************************************************\n//Change float value snap distance\nglobal proc floatValueChangedVertSnapDistance()\n{\n\tglobal string $m341_vertSnapDistanceFloatFieldName;\n\tglobal float $m341_userInputVertSnapper;\t\t\t\n\t//Query the value from the float field and store it in a variable.\n\t$m341_userInputVertSnapper = `floatField -query -value $m341_vertSnapDistanceFloatFieldName`;\n\tprint(\"Snap distance set to \"+ $m341_userInputVertSnapper);\n}\n//***********************************************************************************************************************************************************\n//Load\nglobal proc vertSnapper_Load()\n{\n\ttoggleSelMode;\n\ttoggleSelMode;\n\tselectMode -object;\n\t\n\tglobal string $m341_vertSnap_Selected_OBJ[];\n\t$m341_vertSnap_Selected_OBJ = `ls -sl`;\n\t\n\tglobal string $m341_TextFieldName_vertSnapper;\n\ttextField -e -text $m341_vertSnap_Selected_OBJ -placeholderText \"load mesh to snap to\" $m341_TextFieldName_vertSnapper;\n\tprint (\"Mesh to snap to: \"+ $m341_vertSnap_Selected_OBJ[0] +\"\\n\");\n\t\n\t//Error if nothing selected\n\t//$nothingSelected = `ls -sl`;\n\t//$nothingSelectedSize = `size $nothingSelected`;\n\t//if ($nothingSelectedSize == 0)\n\t//{\n\t//\terror \"Nothing selected\";\n\t//}  \n}\n//***********************************************************************************************************************************************************\n//Snap Verts\nglobal proc snap_Verts()\n{\n\tglobal float $m341_userInputVertSnapper;\t\t\t\n\tglobal int $m341_distance_VertSnapperCheckBox_Value;\n\tglobal int $m341_preserveUV_VertSnapperCheckBox_Value;\n\t$m341_distance_VertSnapperCheckBox_Value = `checkBox -query -value distance_VertSnapperCheckBox`;\n\t$m341_preserveUV_VertSnapperCheckBox_Value = `checkBox -query -value preserveUV_VertSnapperCheckBox`;\n\t//First update the snap value\n\tfloatValueChangedVertSnapDistance();\n\t\n\t//Gets the object name to snap to from the UI and then runs a for loop for each item in the vert array using the below global proc.\n\tConvertSelectionToVertices;\n\tglobal string $m341_TextFieldName_vertSnapper;\n\tstring $verts[] = `ls -sl -fl`;\n\tstring $objectToSnapTo = `textField -q -text $m341_TextFieldName_vertSnapper`;\n\t\n\tfor ($Item in $verts)\n\t{\n\t\tsnapToClosestVert $Item $objectToSnapTo;\n\t}\n\n\tif ($m341_distance_VertSnapperCheckBox_Value == 1)\n\t{\n\t\tprint (\"Snapping complete using distance \" + $m341_userInputVertSnapper);\n\t}\n\t\n\telse\n\t{\n\t\tprint \"Snaping complete using distance 100,000\";\n\t}\n}\n\nglobal proc snapToClosestVert (string $verts, string $objectToSnapTo)\n{\n\tglobal float $m341_userInputVertSnapper;\t\t\t\n\t//Find the world space position of the vert\n\tfloat $vertPosition[] = `pointPosition -w $verts`;\n\t\n\t//Find out how many verts in the the object to snap to\n\tint $snapTo_VertCount[] = `polyEvaluate -v $objectToSnapTo`;\n\t\t\n\t//Get the shape of the object to snap to\n\tstring $obj_Shape[] = `listRelatives -f -s $objectToSnapTo`;\n\t\n\tint $Item;\n\tfloat $closestDistance = 10000;\n\tstring $closestVert;\n\tfloat $closestPosition[];\n\t\n\t//for each vert in the object to snap to\n\tfor ($Item = 0; $Item < $snapTo_VertCount[0]; $Item++)\n\t\t{\n\t\t\t//find the world space position of the vert on the object to snap to\n\t\t\tfloat $snapTo_VertPos[] = `pointPosition -w ($obj_Shape[0] + \".vtx[\" + $Item + \"]\")`;\n\t\t\n\t\t\t//find the difference between the two positions.\n\t\t\tfloat $xValue = $vertPosition[0] - $snapTo_VertPos[0];\n\t\t\tfloat $yValue = $vertPosition[1] - $snapTo_VertPos[1];\n\t\t\tfloat $zValue = $vertPosition[2] - $snapTo_VertPos[2];\n\t\t\tvector $difference = <<$xValue, $yValue, $zValue>>;\n\t\t\n\t\t\t//find the distance between the two points\n\t\t\tfloat $distance = `mag $difference`;\n\t\t\t\n\t\t\t//if the distance between the verts is lower than the distance between the previous vert.\n\t\t\tif ($distance < $closestDistance)\n\t\t\t{\n\t\t\t\t//store the closest distance so far.\n\t\t\t\t$closestDistance = $distance;\n\t\t\t\t//store the name of the closest vertex so far\n\t\t\t\t$closestVert = ($obj_Shape[0] + \".vtx[\" + $Item + \"]\");\n\t\t\t\t//store the closest position so far.\n\t\t\t\t$closestPosition = $snapTo_VertPos;\n\t\t\t}\t\t\n\t\t}\n\tglobal int $m341_distance_VertSnapperCheckBox_Value;\n\tif ($m341_distance_VertSnapperCheckBox_Value == 0)\n\t{\n\t\tglobal float $m341_userInputVertSnapper;\t\t\t\t\n\t\t$m341_userInputVertSnapper = 100000;\n\t}\n\t\n\tif ($closestDistance < $m341_userInputVertSnapper)\n\t{\n\t\tglobal int $m341_preserveUV_VertSnapperCheckBox_Value;\n\t\tif ($m341_preserveUV_VertSnapperCheckBox_Value == 1)\n\t\t{\n\t\t\tmove -ws -puv $closestPosition[0] $closestPosition[1] $closestPosition[2] $verts;\n\t\t\trefresh;\n\t\t}\n\t\telse\n\t\t{\n\t\t\tmove -ws $closestPosition[0] $closestPosition[1] $closestPosition[2] $verts;\n\t\t\trefresh;\n\t\t}\n\t}\n}\n\n\n//***********************************************************************************************************************************************************\n//Window and Buttons\nglobal proc launchVertSnapperInterface()\n{\n\tglobal string $m341_vertSnapDistanceFloatFieldName = \"\";\n\tglobal float $m341_userInputVertSnapper = 4;\n\tglobal string $m341_vertSnap_Selected_OBJ[];\n\tglobal string $m341_TextFieldName_vertSnapper;\n\t\t\t\n\twindow -title \"vertSnapper\" -toolbox true -sizeable false -resizeToFitChildren true maVertSnapper;\n\tcolumnLayout -adjustableColumn true;\n\ttext -label \"by Malcolm341\";\n\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 1 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 50 -columnWidth 2 30;\n\ticonTextButton -style \"textOnly\"\n\t\t-labelOffset 30\n\t\t-label \"click here for help\"\n\t\t-backgroundColor 0.51 0.51 0.51\n\t\t-width 124\n\t\t-height 15\n\t\t-command \"helpMaVertSnapper;\";\n\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 2 -columnAttach2 \"left\" \"left\" -columnOffset2 0 0 -columnWidth 1 84 -columnWidth 2 30;\n\t\tcheckBox -value 0 -label \"preserve UVs\" preserveUV_VertSnapperCheckBox;\n\t\t\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 2 -columnAttach2 \"left\" \"left\" -columnOffset2 0 0 -columnWidth 1 84 -columnWidth 2 30;\n\t\tcheckBox -value 1 -label \"snap radius\" distance_VertSnapperCheckBox;\n\t\t$m341_vertSnapDistanceFloatFieldName = `floatField -value 4 -minValue 0 -precision 1 -width 38 -changeCommand \"floatValueChangedVertSnapDistance();\"`;\n\t\t\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\t$m341_TextFieldName_vertSnapper = `textField -text $m341_vertSnap_Selected_OBJ -placeholderText \"load mesh to snap to\" -width 124 -changeCommand \"textValueChanged_Renamer();\"`;\n\t\n\tsetParent ..;\t\n\trowColumnLayout -numberOfRows 1;\n\ticonTextButton -style \"textOnly\"\n\t\t-label \"load\"\n\t\t-backgroundColor 0.51 0.51 0.51\n\t\t-width 40\n\t\t-command \"vertSnapper_Load;\"\n\t\t-commandRepeatable 0;\n\t\tseparator -width 1 -style \"none\";\n\t\t\t\t\n\ticonTextButton -style \"textOnly\"\n\t\t-label \"snap verts\"\n\t\t-labelOffset 30\n\t\t-backgroundColor 0.72 0.68 0.39\n\t\t-width 83\n\t\t-command \"snap_Verts;\"\n\t\t-commandRepeatable 1;\n\t\t\n\tshowWindow;\n}\n//***********************************************************************************************************************************************************\n//*Launch Window\n//***********************************************************************************************************************************************************\nif (`window -exists maVertSnapper`)\n{\n\tdeleteUI maVertSnapper;\n}\n\nlaunchVertSnapperInterface();\nprint \"vertSnapper toolbox loaded\";\n" 
