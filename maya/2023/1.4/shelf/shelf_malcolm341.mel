        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Replace Material from Clipboard v1.0\n\n{\n\tstring $faces[] = `ls -sl`;\n\tstring $isObjSelected[] = `filterExpand -sm 12`;\n\tint $sizeIsObjSelected = `size $isObjSelected`;\n\n\t//Replace if object mode selected\n\tif ($sizeIsObjSelected == 1)\n\t{\n\t\tglobal string $m341_copyShadingGroup[];\n\t\tsets -e -forceElement $m341_copyShadingGroup;\t\n\t\tprint \"Pasted material from clipboard\";\n\t}\n\t\n\telse\n\t{\n\t\t//Error if more than one face selected\n\t\tstring $errorFaces[] = `filterExpand -sm 34`;\n\t\tint $sizeErrorFaces = `size $errorFaces`;\n\t\tif(size($errorFaces)!= 1)\n\t\t{\n    \t\terror \"Please select exactly one face or one object\";    \n\t\t}\n\t\t\n\t\tglobal string $smid_SelectedFace[];\n\t\tConvertSelectionToFaces;\n\t\t$smid_SelectedFace = `ls -fl -sl`;\n\n\t\ttoggleSelMode;\n\t\ttoggleSelMode;\n\t\tselectMode -object;\n\t\tglobal string $smid_ObjectName[];\n\t\t$smid_ObjectName = `ls -sl`;\n\t\t\n\t\t//If only one material for whoel object replace\n\t\thyperShade -smn;\n\t\tstring $numbMats[] = `ls -sl`;\n\t\tint $sizeNumbMats = `size $numbMats`;\n\t\tif ($sizeNumbMats == 1)\n\t\t{\n\t\t\tselect $smid_ObjectName;\n\t\t\tglobal string $m341_copyShadingGroup[];\n\t\t\tsets -e -forceElement $m341_copyShadingGroup;\n\t\t\tprint \"Replaced material from clipboard\";\n\t\t}\n\t\t\n\t\telse\n\t\t{\n\t\t\tConvertSelectionToFaces;\n\t\t\tselect $smid_SelectedFace;\n   \t\t\thyperShade -smn \"\";\n\t\t\t$smid_Mat_Name = `ls -sl`;\n    \t\t$smid_SG_Name = `listSets -type 1 -object $smid_SelectedFace[0]`;\n\t\t\tselect -clear;\n\t    \tselect -add $smid_SG_Name;\n\t\t\tstring $smid_AllSelectedFaces[] = `ls -fl -sl`;\n\t\t\tstring $smid_FilteredList[];\n   \n\t\t\tfor ($smid_Item in $smid_AllSelectedFaces)\n\t\t\t{\n\t\t\t\tstring $smid_NameMatch = `match $smid_ObjectName[0] $smid_Item`;\n\t        \tif ($smid_NameMatch == $smid_ObjectName[0])    \n\t\t    \t{        \n    \t\t\t\tint $smid_Index = (size($smid_FilteredList));\n        \t\t\t$smid_FilteredList[$smid_Index] = $smid_Item;\n\t\t    \t}\n\t\t\t}\n\n\t\t\tselect -replace $smid_FilteredList;\n\t\t\t//Set component mode to faces\n\t\t\tchangeSelectMode -component;\n\t\t\tsetComponentPickMask \"Facet\" true;\n\t\t\tselectType -ocm -alc false;\n\t\t\tselectType -ocm -facet true;\n\t\t\tselectType -sf false -se false -suv false -cv false;\n\n\t\t\t//Paste material\n\t\t\t{\n\t\t\t\tglobal string $m341_copyShadingGroup[];\n\t\t\t\tsets -e -forceElement $m341_copyShadingGroup;\t\n\t\t\t\t$faces = `ls -sl`;\n\t\t\t\tselect $smid_ObjectName;\n\t\t\t\t//Set component mode to faces\n\t\t\t\tchangeSelectMode -component;\n\t\t\t\tsetComponentPickMask \"Facet\" true;\n\t\t\t\tselectType -ocm -alc false;\n\t\t\t\tselectType -ocm -facet true;\n\t\t\t\tselectType -sf false -se false -suv false -cv false;\n\t\t\t\tselect $faces;\n\t\t\t\tprint \"Replaced material from clipboard\";\n\t\t\t}\n\t\t}\t\t\t\n\t}\n}\n\n" 
