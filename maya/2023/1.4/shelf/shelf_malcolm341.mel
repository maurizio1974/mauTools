width 61\n\t\t-command \"m341_uvMapper_CreateUVsangle30;\"\n\t\t-commandRepeatable 1;\n\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\t\tcheckBox -value 1 -label \"fix map1 overlap\" -height 15 fixMap1Overlap_CheckBox;\n\t\t\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\t\tcheckBox -value 1 -label \"unfold UVs\" -height 15 unfold_CheckBox;\n\t\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\t\tcheckBox -value 1 -label \"spin  \" -height 15 spin_CheckBox;\n\t\tcheckBox -value 1 -label \"scale\" -height 15 scale_CheckBox;\n\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 2 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 67 -columnWidth 2 30;\n\t\toptionMenu -w 118 -label \"map size\" lightMaperMapSizeUI;\n\t\tmenuItem -label \"4096\";\n\t\tmenuItem -label \"2048\";\n\t\tmenuItem -label \"1024\";\n\t\tmenuItem -label \"512\";\n\t\tmenuItem -label \"256\";\n\t\tmenuItem -label \"128\";\n\t\tmenuItem -label \"64\";\n\t\tmenuItem -label \"32\";\n\t\toptionMenu -edit -select 2 lightMaperMapSizeUI;\n\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 2 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 67 -columnWidth 2 30;\n\t\toptionMenu -w 119 -label \"padding\" lightMapperUVSpacerUI;\n\t\tmenuItem -label \"256 pix\";\n\t\tmenuItem -label \"128 pix\";\n\t\tmenuItem -label \"64 pix\";\n\t\tmenuItem -label \"32 pix\";\n\t\tmenuItem -label \"16 pix\";\n\t\tmenuItem -label \"8 pix\";\n\t\tmenuItem -label \"4 pix\";\n\t\toptionMenu -edit -select 4 lightMapperUVSpacerUI;\n\t\t\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 1 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 50 -columnWidth 2 30;\n\ticonTextButton -style \"textOnly\"\n\t\t-labelOffset 30\n\t\t-label \"layout UVs\"\n\t\t-backgroundColor 0.72 0.68 0.39\n\t\t-width 124\n\t\t-command \"m341_lightMapper_LayoutUVs;\"\n\t\t-commandRepeatable 1;\n\t\t\n\tsetParent ..;\t\t\t\n\trowColumnLayout -numberOfRows 1;\n\ticonTextButton -style \"textOn