        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Snap Transform to Component Center v1.1\n\n\n//*******************************************************************************************************\n//Fix Meters\nglobal proc cpasMeters()\n{\n\t//If Maya set to meters divide transform by 10,000\n\tstring $mayaUnits = `currentUnit -q`;\n\tif (`gmatch $mayaUnits \"m\"`)\n\t{\n\t\t//Get move tool POS\n\t\tfloat $locatorTargetPOS[] = `manipMoveContext -q -position \"Move\"`;\n\t\t//Divide by 10,000 for meters\n\t\t$locatorTargetPOSMeter[0] = $locatorTargetPOS[0] /= 10000;\n\t\t$locatorTargetPOSMeter[1] = $locatorTargetPOS[1] /= 10000;\n\t\t$locatorTargetPOSMeter[2] = $locatorTargetPOS[2] /= 10000;\n\t\t//Move object\n\t\txform -ws -t $locatorTargetPOSMeter[0] $locatorTargetPOSMeter[1] $locatorTargetPOSMeter[2];\n\t}\n}\n\nglobal proc m341_createObjectAtFaceCenters()\n{\n\t//Store faces and object\n\tstring $selectedFaces[] = `filterExpand -expand true -sm 34`;\n\tint $sizeSelectedFaces = `size $selectedFaces`;\n\tstring $selectedEdges[] = `filterExpand -expand true -sm 32`;\n\tint $sizeSelectedEdges = `size $selectedEdges`;\n\tstring $selectedVerts[] = `filterExpand -expand true -sm 31`;\n\tint $sizeSelectedVerts = `size $selectedVerts`;\n\t\n\tstring $selectedOBJ[] = `filterExpand -expand true -sm 12`;\n\tint $sizeSelectedOBJ = `size $selectedOBJ`;\n\t\n\tif ($sizeSelectedFaces > 0 && $sizeSelectedOBJ == 1)\n\t{\n    \tfor ($item in $selectedFaces)\n\t\t{\n\t\t\tselect $item;\n\t\t\t//Switch to move tool component mode\n\t\t\tMoveTool;\n\t\t\tmanipMoveContext -edit -mode 10 Move;\n\t\t\t//Get move tool location\n\t\t\tfloat $locatorTargetPOS[] = `manipMoveContext -q -position \"Move\"`;\n\t\t\t//Get move tool angle and convert radius to degrees\n\t\t\tfloat $locatorTargetAngle[] = `manipMoveContext -q -orientAxes \"Move\"`;\n\t\t\t$locatorTargetAngle[0] = rad_to_deg($locatorTargetAngle[0]);\n\t\t\t$locatorTargetAngle[1] = rad_to_deg($locatorTargetAngle[1]);\n\t\t\t$locatorTargetAngle[2] = rad_to_deg($locatorTargetAngle[2]);\n\t\n\t    \t//Duplicate object and move it\n\t\t\tselect $selectedOBJ;\n\t\t\tbakeCustomToolPivot 1 1;\n\t\t\tduplicate -rr;\n\t    \txform -ws -t $locatorTargetPOS[0] $locatorTargetPOS[1] $locatorTargetPOS[2];\n    \t\txform -rotation $locatorTargetAngle[0] $locatorTargetAngle[1] $locatorTargetAngle[2];\n\t\t\t//Rotate 90 to make Y up\n\t\t\trotate -r -os -fo 0 0 -90 ;\n\t\t\t//Switch to object mode to fix people using F8\n\t\t   \ttoggleSelMode;\n\t\t\ttoggleSelMode;\n\t\t\tselectMode -object;\n\t\t\t//Fix units set to meters\n\t\t\tcpasMeters();\n\t\t\t//Set move tool back to object mode\n\t\t\tmanipMoveContext -edit -mode 0 Move;\n\t\t}\n\t}\n\t\n\tif ($sizeSelectedEdges > 0 && $sizeSelectedOBJ == 1)\n\t{\n\t   \tfor ($item in $selectedEdges)\n\t\t{\n\t\t\tselect $item;\n\t\t\t//Switch to move tool component mode\n\t\t\tMoveTool;\n\t\t\tmanipMoveContext -edit -mode 10 Move;\n\t\t\t//Get move tool location\n\t\t\tfloat $locatorTargetPOS[] = `manipMoveContext -q -position \"Move\"`;\n\t\t\t//Get move tool angle and convert radius to degrees\n\t\t\tfloat $locatorTargetAngle[] = `manipMoveContext -q -orientAxes \"Move\"`;\n\t\t\t$locatorTargetAngle[0] = rad_to_deg($locatorTargetAngle[0]);\n\t\t\t$locatorTargetAngle[1] = rad_to_deg($locatorTargetAngle[1]);\n\t\t\t$locatorTargetAngle[2] = rad_to_deg($locatorTargetAngle[2]);\n\t\n\t    \t//Duplicate object and move it\n\t\t\tselect $selectedOBJ;\n\t\t\tbakeCustomToolPivot 1 1;\n\t\t\tduplicate -rr;\n\t    \txform -ws -t $locatorTargetPOS[0] $locatorTargetPOS[1] $locatorTargetPOS[2];\n    \t\txform -rotation $locatorTargetAngle[0] $locatorTargetAngle[1] $locatorTargetAngle[2];\n\t\t\t//Rotate 90 to make Y up\n\t\t\trotate -r -os -fo 0 0 -90 ;\n\t\t\t//Switch to object mode to fix people using F8\n\t\t   \ttoggleSelMode;\n\t\t\ttoggleSelMode;\n\t\t\tselectMode -object;\n\t\t\t//Fix units set to meters\n\t\t\tcpasMeters();\n\t\t\t//Set move tool back to object mode\n\t\t\tmanipMoveContext -edit -mode 0 Move;\n\t\t}\t\n\t}\n\t\n\tif ($sizeSelectedVerts > 0 && $sizeSelectedOBJ == 1)\n\t{\n\t   \tfor ($item in $selectedVerts)\n\t\t{\n\t\t\tselect $item;\n\t\t\t//Switch to move tool component mode\n\t\t\tMoveTool;\n\t\t\tmanipMoveContext -edit -mode 10 Move;\n\t\t\t//Get move tool location\n\t\t\tfloat $locatorTargetPOS[] = `manipMoveContext -q -position \"Move\"`;\n\t\t\t//Get move tool angle and convert radius to degrees\n\t\t\tfloat $locatorTargetAngle[] = `manipMoveContext -q -orientAxes \"Move\"`;\n\t\t\t$locatorTargetAngle[0] = rad_to_deg($locatorTargetAngle[0]);\n\t\t\t$locatorTargetAngle[1] = rad_to_deg($locatorTargetAngle[1]);\n\t\t\t$locatorTargetAngle[2] = rad_to_deg($locatorTargetAngle[2]);\n\t\n\t    \t//Duplicate object and move it\n\t\t\tselect $selectedOBJ;\n\t\t\tbakeCustomToolPivot 1 1;\n\t\t\tduplicate -rr;\n\t    \txform -ws -t $locatorTargetPOS[0] $locatorTargetPOS[1] $locatorTargetPOS[2];\n    \t\txform -rotation $locatorTargetAngle[0] $locatorTargetAngle[1] $locatorTargetAngle[2];\n\t\t\t//Rotate 90 to make Y up\n\t\t\trotate -r -os -fo 0 0 -90 ;\n\t\t\t//Switch to object mode to fix people using F8\n\t\t   \ttoggleSelMode;\n\t\t\ttoggleSelMode;\n\t\t\tselectMode -object;\n\t\t\t//Fix units set to meters\n\t\t\tcpasMeters();\n\t\t\t//Set move tool back to object mode\n\t\t\tmanipMoveContext -edit -mode 0 Move;\n\t\t}\t\n\t}\t\n}\nm341_createObjectAtFaceCenters();\n\t\t\n\n\n\t\n\t\n\t\n\n\n" 
