        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\nglobal string $m341_checkerMapperVersion;\n$m341_checkerMapperVersion = \"checkerMap v1.5\";\n\n//***********************************************************************************************************************************************************\n//* global variables, check if default checker value is zero and set it to 1 if the tool was loaded from fresh boot\n//***********************************************************************************************************************************************************\nglobal float $maGIncrementP2DU;\nglobal float $maGIncrementP2DV;\n\nif ($maGIncrementP2DU == \"0\")\n{\n\tglobal float $maGIncrementP2DU;\n\tglobal float $maGIncrementP2DV;\n\t$maGIncrementP2DU = 1;\n\t$maGIncrementP2DV = 1;\n}\n\n//***********************************************************************************************************************************************************\n//Help\nglobal proc helpmaCustomCheckerMap()\n{\n\tglobal string $m341_customChecker_HelpWindow;\n\n\tif (`window -exists $m341_customChecker_HelpWindow`)\n\t{\n\t\tglobal string $m341_customChecker_HelpWindow;\n\t\tdeleteUI $m341_customChecker_HelpWindow;\n\t}\n\t\n\t$m341_customChecker_HelpWindow = `window -title \"checkerMap Help\" -leftEdge 900 -topEdge 115 -toolbox true -sizeable false -resizeToFitChildren false -width 800 -height 465`;\n\t\t\n\tpaneLayout -configuration \"horizontal2\";\n\tscrollField -text \"find checker folder button:\\nClick to open the folder where you can place your own custom checker map texture for the tool to use.\\nThe texture must be named checker.png\\nThe folder path for PC and Mac will be different. On PC C:\\\\matemp\\\\checker.png on Mac /Users/Shared/matemp\\nYou can place two checker textures in the same folder for the tool to use named checker.png and checker2.png\\n\\nchecker map1 button:\\nClick to toggle the checker map display on/off. Note that the checker map won't appear in Maya until you've placed a file named checker.png in the correct folder. Press the +/- buttons to increase or decrease checker tiling.\\n\\nchecker map2 button:\\nClick to toggle the alternate checker map display on/off. Note that the checker map won't appear in Maya until you've placed a file named checker2.png in the correct folder. Press the +/- buttons to increase or decrease checker tiling.\\n\\nbase color mode button:\\nClick to toggle base color mode on/off.\\n\\nchk1, chk2, chk3, chk4 buttons:\\nClick to procedurally generate four different styles of checker maps. Each map will overwrite checker.png and become the main checker texture the tool uses, if you're using a custom checker texture it will be overwritten.\\nProcedurally generating checker maps doesn't work on Maya LT unfortunately.\\n\\nIf you save the scene with the checker map turned on the tool will automatically disable the checker map and save the scene again to prevent you from losing all your material assignments. If you're using the Maya Student Version the save prompt will appear twice and you should click Continue twice to prevent scene corruption.\\n\\nYoutube video here\\nhttps://youtu.be/tiiLlOA1v9Q?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\"\n\t-wordWrap true\n\t-enableBackground true\n\t-backgroundColor 0.23 0.23 0.23;\n\t\t\n\tshowWindow $m341_customChecker_HelpWindow;\n}\n\n//***********************************************************************************************************************************************************\n//Save Fix Materials\nglobal proc runScriptJobProc_SaveFix()\n{\n\t//Switch file name global variables\n\tglobal int $m341_clickButton1;\n\tglobal int $m341_clickButton2;\n\tglobal string $m341_checkerImage;\n\t\n\t$maCheckerOriginalSelection = `ls -sl`;\n\t\n\t//Global variables\n\tglobal string $shadingEngines[];\n\tglobal string $shadingEngineRemoveDupes[];\n\tglobal string $maMaterialsToPasteBack[];\n\tglobal int $maToggleCustomChecker;\n\t$maToggleCustomChecker = !$maToggleCustomChecker;\n\tglobal int $maToggleCheckerBaseColour;\t\t\n\tglobal int $checkIfCheckerMapExists;\n\t$checkIfCheckerMapExists = 0;\n\tglobal int $maCheckerJobNum;\n\tglobal int $maCheckerJobNumAutoSaveFix;\n\tglobal string $materials[];\n\tglobal string $maCheckerComponents[];\n\tglobal int $maCheckerCounter;\n\t$maCheckerCounter = 0;\n\t\n\tif ($maToggleCustomChecker == 1)\n\t{\n\t\t//Disconnect initalParticle to keep array from adding two materials instead of one when lambert1 applied to face on selected object\n\t\tstring $checkerParticleTest[] = `listConnections -d off -s on initialParticleSE`;\n\t\t$checkerParticleTestResult = `gmatch $checkerParticleTest[0] \"lambert1\"`;\n\t\tif ($checkerParticleTestResult == 1)\n\t\t{\n\t\t\tdisconnectAttr lambert1.outColor initialParticleSE.surfaceShader;    \n\t\t}\n\t\n\t\n\t\t//Store components or objects in list so you can just put checker on their materials\n\t\tglobal string $maCheckerComponents[];\n\t\t$maCheckerComponents = `ls -sl`;\n\t\n\t\t//Create folder on hard drive for custom checker texture\n\t\tstring $maCurrentMayaVersionMac = `about -mac`;\n\t\tif($maCurrentMayaVersionMac == 1)\n\t\t{\n\t\t\tsysFile -makeDir \"/Users/Shared/matemp\";\n\t\t}\t\n\t\telse\n\t\t{\n\t\t\tsysFile -makeDir \"C:/matemp\";\n\t\t}\n\t\t\n\t\t//turn on textures for all modeling windows\n\t\tsetFocus modelPanel1;\n\t\tdisplayStyle -textured;\n\t\tsetFocus modelPanel2;\n\t\tdisplayStyle -textured;\n\t\tsetFocus modelPanel3;\n\t\tdisplayStyle -textured;\n\t\tsetFocus modelPanel4;\n\t\tdisplayStyle -textured;\t\n\t\n\t\t//Delete the checker map rig so it doens't double up\n\t\t//string $maCheckerMapSelectedOBJ[] = `ls -sl -o -dag -s`;\n\t\t//hyperShadePanelMenuCommand(\"hyperShadePanel1\", \"deleteUnusedNodes\");\n\t\tstring $searchSceneForCheckerMat[] = `ls -mat`;\n\t\tfor ($Item in $searchSceneForCheckerMat)\n\t\t{\n\t\t\tif (`gmatch $Item \"ma_checker_*\"`)\n\t\t\t{\n\t\t\t\tselect \"ma_checker*\";\n\t\t\t\tdelete;\n\t\t\t}\n\t\t}\n\t\t\t\t\t\n\t\t//Set checker map switch to on\n\t\tglobal int $checkIfCheckerMapExists;\n\t\t$checkIfCheckerMapExists = 1;\n\t\n\t\t//Create checker material\n\t\tshadingNode -asShader lambert -name ma_checker_map_m;\n\t\t//setAttr \"ma_checker_map_m.diffuse\" 1;\n\t\t$maUVMaterialSelected = `ls -sl`;\n\t\t\n\t\t//Create checker file node and plug in texture from c:drive\n\t\tshadingNode -asTexture -isColorManaged file;\n\t\trename \"ma_checker_map_f\";\n\t\t$maFileNodeSelected = `ls -sl`;\n\t\tstring $maCurrentMayaVersionMac2 = `about -mac`;\n\t\tif($maCurrentMayaVersionMac2 == 1)\n\t\t{\n\t\t\tsetAttr -type \"string\" ($maFileNodeSelected[0] + \".fileTextureName\") (\"/Users/Shared/matemp/\" + $m341_checkerImage);\n\t\t}\t\n\t\telse\n\t\t{\n\t\t\tsetAttr -type \"string\" ($maFileNodeSelected[0] + \".fileTextureName\") (\"C:/matemp/\" + $m341_checkerImage);\n\t\t}\n\t\tconnectAttr -f ($maFileNodeSelected[0] + \".outColor \") ($maUVMaterialSelected[0] + \".color\");\n\t\t\n\t\t//Create checker place 2D node\n\t\tshadingNode -asUtility place2dTexture;\n\t\trename \"ma_checker_map_p2d\";\n\t\t$maPlace2DNodeSelected = `ls -sl`;\n\t\t\n\t\t//Connect place 2D node to file node\n\t\tconnectAttr -f ($maPlace2DNodeSelected[0] + \".repeatUV \") ($maFileNodeSelected[0] + \".repeatUV\");\n\t\tconnectAttr -f ($maPlace2DNodeSelected[0] + \".outUV \") ($maFileNodeSelected[0] + \".uv\");\n\t\n\t\t\n\t\t//Check if anything selected before script is run\n\t\tfloat $sizeOfmaCheckerComponents = size($maCheckerComponents);\n\t\tif ($sizeOfmaCheckerComponents == 0)\n\t\t{\n\t\t\t//Select all meshes and get what materials applied to prevent 2020 standard surface bug\n\t\t\tselect `ls -typ mesh`;\t\n\t\t\thyperShade -smn \"\";\n\t\t\tglobal string $materials[];\n\t\t\t$materials = `ls -sl`;\n\t\t\tselect $materials;\n\t\t\tglobal string $maMaterialsToPasteBack[];\n\t\t\t$maMaterialsToPasteBack = `ls -sl`;\n\t\t\tselect -cl;\n\t\t}\n\t\t\n\t\telse\n\t\t{\n\t\t\t//Select only some materials and store them in a variable\n\t\t\tglobal string $maCheckerComponents[];\n\t\t\tselect $maCheckerComponents;\n\t\t\thyperShade -smn \"\";\n\t\t\tglobal string $materials[];\n\t\t\t$materials = `ls -sl`;\n\t\t\tselect $materials;\n\t\t\tglobal string $maMaterialsToPasteBack[];\n\t\t\t$maMaterialsToPasteBack = `ls -sl`;\n\t\t\tselect -cl;\n\t\t}\n\t\n\t\t\n\t\t//get the list of shading groups and store the number of shading groups\n\t\tstring $shadingEngines[] = listConnections(\"-type\",\"shadingEngine\", $materials);\n\t\t$shadingEngineRemoveDupes = stringArrayRemoveDuplicates($shadingEngines);\n\t\n\t\t\n\t\t//Set color management of scene to off \n\t\t//setAttr -type \"string\" ($maFileNodeSelected[0] + \".colorSpace \") sRGB;\n\t\t//setAttr -type \"string\" ($maFileNodeSelected[0] + \".colorSpace \") Raw;\n\t\tcolorManagementPrefs -edit -cmEnabled 0;\n\t\t\n\t\t\n\t\t\n\t\n\t\t//Turn checker map on by plugging checker map into object's shading groups\n\t\tfor ($maCheckerItem in $shadingEngineRemoveDupes)\n\t\t{\n\t\t\tglobal int $maCheckerCounter;\n\t\t\tconnectAttr -f ($maUVMaterialSelected[0] + \".outColor \") ($shadingEngineRemoveDupes[$maCheckerCounter] + \".surfaceShader\");\n\t\t\t$maCheckerCounter = $maCheckerCounter += 1;\n\t\t}\n\t\t\n\t\t//Remember checker tile from last time pull from global variable\n\t\tglobal float $maGIncrementP2DU;\n\t\tglobal float $maGIncrementP2DV;\n\t\tsetAttr ma_checker_map_p2d.repeatU $maGIncrementP2DU;\n\t\tsetAttr ma_checker_map_p2d.repeatV $maGIncrementP2DV;\n\t\t\n\t\tglobal int $maRememberBaseColour;\n\t\tif ($maRememberBaseColour == 1)\n\t\t{\n\t\t\tbuttonCheckMapBaseColour();\n\t\t}\n\t\t\n\t\t//Turn ON UV editor image options so you can see wireframe 2019\n\t\tstring $maCurrentMayaYearUVFix = `about -product`;\n\t\tif(`gmatch $maCurrentMayaYearUVFix \"*2019*\"`)\n\t\t{\n\t\t\tif ((size(`textureWindow -query -parent polyTexturePlacementPanel1`)) == 0)\n\t\t\t{\n\t\t\t//Do nothing\n\t\t\t}\n\t\t\telse\n\t\t\t{\t\t\n\t\t\t\t//Turn OFF CHECKER\n\t\t\t\ttextureWindowDisplayCheckered(1,0);\n\t\t\t\ttxtWndUpdateEditor(\"polyTexturePlacementPanel1\", \"textureWindow\", \"null\", 101);\n\t\t\t\t//TURN ON SHADED 2019\n\t\t\t\t//textureWindow -edit -solidMapPerShell 0 -displaySolidMap 1 polyTexturePlacementPanel1; DisplayUVShaded;\n\t\t\t\t//DIM IMAGE 2019\n\t\t\t\tif(`getModifiers`%2) performTextureViewDimImageOptions 1; else{textureWindowImageDimming(1,1); performTextureViewDimImageOptions 0;} txtWndUpdateEditor(\"polyTexturePlacementPanel1\", \"textureWindow\", \"null\", 101);\n\t\t\t\tuvTbImageDimmingChangedCallback;\n\t\t\t\ttextureWindow -edit -ibc 0.6 0.6 0.6 polyTexturePlacementPanel1;\n\t\t\t\t//TURN ON IMAGE 2019\n\t\t\t\t$maCheckIfImageOn2019 = `textureWindow -q -imageDisplay polyTexturePlacementPanel1`;\n\t\t\t\tif ($maCheckIfImageOn2019 == 0)\n\t\t\t\t{\n\t\t\t\t\tToggleUVTextureImage;\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t\t\n\t\t//Turn ON UV editor image options so you can see wireframe 2018\n\t\tif(`gmatch $maCurrentMayaYearUVFix \"*2018*\"`)\n\t\t{\n\t\t\tif ((size(`textureWindow -query -parent polyTexturePlacementPanel1`)) == 0)\n\t\t\t{\n\t\t\t//Do nothing\n\t\t\t}\n\t\t\telse\n\t\t\t{\n\t\t\t\t//Turn OFF CHECKER\n\t\t\t\ttextureWindow -edit -displayCheckered 0 polyTexturePlacementPanel1;\n\t\t\t\ttxtWndUpdateEditor(\"polyTexturePlacementPanel1\", \"textureWindow\", \"null\", 101);\n\t\t\t\t//TURN ON SHADED 2018\n\t\t\t\t//textureWindow -edit -solidMapPerShell 0 -displaySolidMap 1 polyTexturePlacementPanel1;\n\t\t\t\t//DIM IMAGE 2018\n\t\t\t\ttextureWindow -edit -imageDim 1 polyTexturePlacementPanel1;\n\t\t\t\tperformTextureViewDimImageOptions 0;\n\t\t\t\ttextureWindow -edit -ibc 0.6 0.6 0.6 polyTexturePlacementPanel1;\n\t\t\t\t//TURN ON IMAGE 2018\n\t\t\t\ttextureWindow -edit -imageDisplay 1 polyTexturePlacementPanel1;\n\t\t\t\tstring $texWinName[] = `getPanel -sty polyTexturePlacementPanel`;\n\t\t\t\ttxtWndUpdateEditor( $texWinName[0], \"textureWindow\", \"null\", 101 );\t\n\t\t\t}\n\t\t}\n\t\t\n\t\t//Turn ON UV editor image options so you can see wireframe 2017\n\t\tif(`gmatch $maCurrentMayaYearUVFix \"*2017*\"`)\n\t\t{\n\t\t\tif ((size(`textureWindow -query -parent polyTexturePlacementPanel1`)) == 0)\n\t\t\t{\n\t\t\t//Do nothing\n\t\t\t}\n\t\t\telse\n\t\t\t{\n\t\t\t\t//TURN OFF CHECKER 2017\n\t\t\t\ttextureWindow -edit -displayCheckered 0 polyTexturePlacementPanel1;\n\t\t\t\ttxtWndUpdateEditor(\"polyTexturePlacementPanel1\", \"textureWindow\", \"null\", 101);\n\t\t\t\t//TURN ON SHADED 2017\n\t\t\t\t//textureWindow -edit -solidMapPerShell 0 -displaySolidMap 1 polyTexturePlacementPanel1;\n\t\t\t\t//DIM IMAGE 2017\n\t\t\t\ttextureWindow -e -imageDim 1 polyTexturePlacementPanel1; performTextureViewDimImageOptions 0;\n\t\t\t\ttextureWindow -edit -ibc 0.6 0.6 0.6 polyTexturePlacementPanel1;\n\t\t\t\t//TURN ON IMAGE 2017\n\t\t\t\ttextureWindow -edit -imageDisplay 1 polyTexturePlacementPanel1;\n\t\t\t\tstring $texWinName[] = `getPanel -sty polyTexturePlacementPanel`;\n\t\t\t\ttxtWndUpdateEditor( $texWinName[0], \"textureWindow\", \"null\", 101 );\n\t\t\t}\n\t\t}\n\t\t\n\t\tprint (\"Custom checker map script job ON \" + $maCheckerCounter + \" materials\");    \n\t}\n\t\n\telse if ($maToggleCustomChecker == 0)\n\t{\n\t\t//Set checker map switch to off\n\t\tglobal int $checkIfCheckerMapExists;\n\t\t$checkIfCheckerMapExists = 0;\n\t\t\n\t\t//Turn off base colour mode to fix viewport bug, change toggle to off to prevent toggle bug\n\t\tsoloMaterial -unsolo;\n\t\tglobal int $maToggleCheckerBaseColour;\n\t\t$maToggleCheckerBaseColour = 0;\n\t\t\n\t\t//Clear the selection\n\t\tselect -cl;\n\t\n\t\t//Turn colour management back on\n\t\tcolorManagementPrefs -edit -cmEnabled 1;\t\t\t\n\t\n\t\t\n\t\n\t\t//Paste back materials from stored names into original shading groups\n\t\tglobal int $maCheckerCounter;\n\t\tfor ($maCheckerItem in $shadingEngineRemoveDupes)\n\t\t{\n\t\t\tglobal int $maCheckerCounter;\n\t\t\tconnectAttr -f ($maMaterialsToPasteBack[$maCheckerCounter] + \".outColor \") ($shadingEngineRemoveDupes[$maCheckerCounter] + \".surfaceShader\");\n\t\t\t$maCheckerCounter = $maCheckerCounter += 1;\n\t\t}\n\t\t\n\t\tstring $searchSceneForCheckerMat[] = `ls -mat`;\n\t\tfor ($Item in $searchSceneForCheckerMat)\n\t\t{\n\t\t\tif (`gmatch $Item \"ma_checker_*\"`)\n\t\t\t{\n\t\t\t\tselect \"ma_checker*\";\n\t\t\t\tdelete;\n\t\t\t}\n\t\t}\n\t\t\n\t\t//Turn off UV editor image options so you don't see checker\n\t\tstring $maCurrentMayaYearUVFix2 = `about -product`;\n\t\tif(`gmatch $maCurrentMayaYearUVFix2 \"*2019*\"`)\n\t\t{\n\t\t\tif ((size(`textureWindow -query -parent polyTexturePlacementPanel1`)) == 0)\n\t\t\t{\n\t\t\t//Do nothing\n\t\t\t}\n\t\t\telse\n\t\t\t{\n\t\t\t//TURN OFF CHECKERS 2019\n\t\t\tif(`getModifiers`%2) performTextureViewCheckerMapOptions 1; else{ textureWindowDisplayCheckered(1,0);} txtWndUpdateEditor(\"polyTexturePlacementPanel1\", \"textureWindow\", \"null\", 101);\n\t\t\t}\n\t\t\t//TURN OFF SHADED 2019\n\t\t\ttextureWindow -edit -displayCheckered 0 -checkerColorMode 0 polyTexturePlacementPanel1;\n\t\t\tDisplayUVWireframe;\n\t\t\t//TURN OFF DIM IMAGE 2019\n\t\t\ttextureWindow -e -imageDim 0 polyTexturePlacementPanel1; performTextureViewDimImageOptions 0;\n\t\t\ttextureWindow -edit -ibc 0.5 0.5 0.5 polyTexturePlacementPanel1;\t\n\t\t}\n\t\tif(`gmatch $maCurrentMayaYearUVFix2 \"*2018*\"`)\n\t\t{\n\t\t\t//TURN OFF CHECKERS 2018\n\t\t\ttextureWindow -edit -displayCheckered 0 -checkerColorMode 0 polyTexturePlacementPanel1;\n\t\t\t//TURN OFF SHADED 2018\n\t\t\tDisplayUVWireframe;\n\t\t\t//TURN OFF DIM IMAGE 2018\n\t\t\ttextureWindow -e -imageDim 0 polyTexturePlacementPanel1; performTextureViewDimImageOptions 0;\n\t\t\ttextureWindow -edit -ibc 0.5 0.5 0.5 polyTexturePlacementPanel1;\t\n\t\t}\n\t\tif(`gmatch $maCurrentMayaYearUVFix2 \"*2017*\"`)\n\t\t{\n\t\t\t//TURN OFF CHECKERS 2017\n\t\t\ttextureWindow -edit -displayCheckered 0 polyTexturePlacementPanel1;\n\t\t\t//TURN OFF SHADED 2017\n\t\t\ttextureWindow -edit -solidMapPerShell 0 -displaySolidMap 0 polyTexturePlacementPanel1;\n\t\t\tDisplayUVWireframe;\n\t\t\t//TURN OFF DIM IMAGE 2017\n\t\t\ttextureWindow -e -imageDim 0 polyTexturePlacementPanel1; performTextureViewDimImageOptions 0;\n\t\t\ttextureWindow -edit -ibc 0.5 0.5 0.5 polyTexturePlacementPanel1;\n\t\t}\n\t\t\n\t\tprint (\"Custom checker map script job OFF \" + $maCheckerCounter + \" materials\");    \n\t}\n\tselect $maCheckerOriginalSelection;\n\tSaveScene;\n\tm341_Custom_CheckerMap();\n}\n\n//***********************************************************************************************************************************************************\n//Auto Save Fix Materials\nglobal proc runScriptJobProc_AutoSaveFix()\n{\n\t//Switch file name global variables\n\tglobal int $m341_clickButton1;\n\tglobal int $m341_clickButton2;\n\tglobal string $m341_checkerImage;\n\t\n\t$maCheckerOriginalSelection = `ls -sl`;\n\t\n\t//Global variables\n\tglobal string $shadingEngines[];\n\tglobal string $shadingEngineRemoveDupes[];\n\tglobal string $maMaterialsToPasteBack[];\n\tglobal int $maToggleCustomChecker;\n\t$maToggleCustomChecker = !$maToggleCustomChecker;\n\tglobal int $maToggleCheckerBaseColour;\t\t\n\tglobal int $checkIfCheckerMapExists;\n\t$checkIfCheckerMapExists = 0;\n\tglobal int $maCheckerJobNum;\n\tglobal int $maCheckerJobNumAutoSaveFix;\n\tglobal string $materials[];\n\tglobal string $maCheckerComponents[];\n\tglobal int $maCheckerCounter;\n\t$maCheckerCounter = 0;\n\t\n\tif ($maToggleCustomChecker == 1)\n\t{\n\t\t//Disconnect initalParticle to keep array from adding two materials instead of one when lambert1 applied to face on selected object\n\t\tstring $checkerParticleTe