        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//uvEdge v1.0\n\n//***********************************************************************************************************************************************************\n//* global variables\n//***********************************************************************************************************************************************************\n//variable to store the number the user inputs into use angle\n\tglobal float $userInputUVSoftenToleranceAngle = 45.1;\n//float field UI name\n\tglobal string $softenAngleFloatFieldName=\"\";\n\n//***********************************************************************************************************************************************************\n//* global procs\n//***********************************************************************************************************************************************************\n//Change Float Value Angle\nglobal proc floatValueChangedUVEdges()\n{\n\tglobal string $softenAngleFloatFieldName;\n\tglobal float $userInputUVSoftenToleranceAngle;\t\t\t\n\t// query the value from the float field and store it in a variable\n\t$userInputUVSoftenToleranceAngle = `floatField -query -value $softenAngleFloatFieldName`;\n\t// print value\n\tprint(\"Soften tolerance angle set to \"+ $userInputUVSoftenToleranceAngle +\"\\n\");\n}\n\n//***********************************************************************************************************************************************************\n//Help\nglobal proc helpmaUVEdges ()\n{\n\tglobal string $m341_uvEdges_HelpWindow;\n\n\tif (`window -exists $m341_uvEdges_HelpWindow`)\n\t{\n\t\tglobal string $m341_uvEdges_HelpWindow;\n\t\tdeleteUI $m341_uvEdges_HelpWindow;\n\t}\n\t\n\t$m341_uvEdges_HelpWindow = `window -title \"uvEdges Help\" -leftEdge 900 -topEdge 115 -toolbox true -sizeable false -resizeToFitChildren false -width 800 -height 160`;\n\t\t\n\tpaneLayout -configuration \"horizontal2\";\n\tscrollField -text \"Click the harden UV edges button to automatically soften all normals then harden UV border edges using the angle tolerence specifed in the Angle field.\\n\\nTurn the soften tolerance tick box off if you want to ignore the Angle setting.\\n\\nClick the show edges button to toggle visability of edges, this can be useful to see how the edge hardness has been set without having to deselect your model.\\n\\nYoutube video here\\nhttps://youtu.be/aNB0Yrd_864?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\"\n\t-wordWrap true\n\t-enableBackground true\n\t-backgroundColor 0.23 0.23 0.23;\n\t\n\tshowWindow $m341_uvEdges_HelpWindow;\n}\n\n//***********************************************************************************************************************************************************\n//Harden UV Edges\nglobal proc button1maUVEdges ()\n{  \n\tfloatValueChangedUVEdges();\n\tglobal float $userInputUVSoftenToleranceAngle;\n\tint $softenToleranceCheckMarkValue = `checkBox -query -value softenToleranceCheckBox`;\n\n\tif ($softenToleranceCheckMarkValue == 1)\n\t{\n\t\ttoggleSelMode;\n\t\ttoggleSelMode;\n\t\tselectMode -object;\n\t\tpolyPerformAction \"polySoftEdge -a 180\" e 0;\n\t\tConvertSelectionToEdges;\n\t\tSelectUVBorderComponents;\n\t\tpolyPerformAction \"polySoftEdge -a 0\" e 0;\n\t\t\n\t\t//openSelectionConstraintWindow;\n\t\t\n\t\tpolySelectConstraint -mode 2 -type 0x0008 -smoothness 1 -angle true -anglebound 0 54;\n\t\tpolyPerformAction \"polySoftEdge -a $userInputUVSoftenToleranceAngle\" e 0;\n\t\tresetPolySelectConstraint;\n\t\t\n\t\t//polygonConstraintUpdate polySelectionConstraintPanel1;\n\t\t//resetPolySelectConstraint; evalDeferred(\"deleteUI polySelectionConstraintPanel1Window\");\n\t\t//polygonConstraintUpdate polySelectionConstraintPanel1;\n\t\t\n\t\ttoggleSelMode;\n\t\ttoggleSelMode;\n\t\tselectMode -object;\n\t\tDeleteHistory;\n\t\tprint \"Normals softened, UV border edges below tolerance hardened\";\n\t}\n\n\telse if ($softenToleranceCheckMarkValue == 0)\n\t{\t\n\t\ttoggleSelMode;\n\t\ttoggleSelMode;\n\t\tselectMode -object;\n\t\tpolyPerformAction \"polySoftEdge -a 180\" e 0;\n\t\tConvertSelectionToEdges;\n\t\tSelectUVBorderComponents;\n\t\tpolyPerformAction \"polySoftEdge -a 0\" e 0;\n\t\t\n\t\t//openSelectionConstraintWindow;\n\t\t\n\t\tpolySelectConstraint -mode 2 -type 0x0008 -smoothness 1 -angle true -anglebound 0 54;\n\t\tpolyPerformAction \"polySoftEdge -a 0\" e 0;\n\t\tresetPolySelectConstraint;\n\t\t\n\t\t//polygonConstraintUpdate polySelectionConstraintPanel1;\n\t\t//resetPolySelectConstraint; evalDeferred(\"deleteUI polySelectionConstraintPanel1Window\");\n\t\t//polygonConstraintUpdate polySelectionConstraintPanel1;\n\t\t\n\t\ttoggleSelMode;\n\t\ttoggleSelMode;\n\t\tselectMode -object;\n\t\tDeleteHistory; \n\t\tprint \"Normals softened, UV border edges hardened\";\n\t}\n}\n//***********************************************************************************************************************************************************\n//Show Edges\nglobal proc button2maUVEdges ()\n{  \n\tstring $maDisplayMode = `displayPref -q -wsa`;\n\n\tif ($maDisplayMode == \"full\")\n\t{\n\t\tdisplayPref -wsa \"none\";\n\t\tprint \"Edge display style changed to NONE\";\n\t}\n\n\telse if ($maDisplayMode == \"none\")\n\t{\n\t\tdisplayPref -wsa \"full\";\n\t\tprint \"Edge display style changed to FULL\";\n\t}\n\n\telse\n\t{\n\t\tdisplayPref -wsa \"full\";\n\t\tprint \"Edge display style defaulted to FULL\";\n\t}\n}\n\n//***********************************************************************************************************************************************************\n//* window and buttons\n//***********************************************************************************************************************************************************\nif (`window -exists maUVEdges`)\n{\n\tdeleteUI maUVEdges;\n};\n\n{\n\twindow -title \"uvEdges\" -toolbox true -sizeable false -resizeToFitChildren true maUVEdges;\n\tcolumnLayout -adjustableColumn true;\n\ttext -label \"by Malcolm341\";\n\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 1 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 50 -columnWidth 2 30;\n\ticonTextButton -style \"textOnly\"\n\t\t-labelOffset 30\n\t\t-label \"click here for help\"\n\t\t-backgroundColor 0.51 0.51 0.51\n\t\t-width 110\n\t\t-height 15\n\t\t-command \"helpmaUVEdges;\";\n\t\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 2 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 67 -columnWidth 2 30;\n\t\tcheckBox -value 1 -width 103 -label \"soften tolerance\" softenToleranceCheckBox;\n\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 2 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 36 -columnWidth 2 30;\n\ttext -label \"Angle\";\n\t$softenAngleFloatFieldName = `floatField -value 45.1 -pre 1 -width 70 -changeCommand \"floatValueChangedUVEdges();\"`;\n\t\t\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 1 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 50 -columnWidth 2 30;\n\ticonTextButton -style \"textOnly\"\n\t\t-labelOffset 30\n\t\t-label \"harden UV edges\"\n\t\t-backgroundColor 0.772 0.521 0.302\n\t\t-width 110\n\t\t-command \"button1maUVEdges;\"\n\t\t-commandRepeatable 0;\n\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 1 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 50 -columnWidth 2 30;\n\ticonTextButton -style \"textOnly\"\n\t\t-labelOffset 30\n\t\t-label \"show edges\"\n\t\t-backgroundColor 0.51 0.51 0.51\n\t\t-width 110\n\t\t-command \"button2maUVEdges;\"\n\t\t-commandRepeatable 0;\n\t\t\t\t\n\tshowWindow;\n\tprint \"uvEdges toolbox loaded\";\n}" 
