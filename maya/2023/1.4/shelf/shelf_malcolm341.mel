        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Normal Map Render v1.0\n\nstring $normalMapApplySelected[] = `ls -sl`;\nfloat $sizeNormLObjs = size($normalMapApplySelected);\n\n\nint $normRlSwitch = 0;\nstring $normalMapGroup[] = `ls -tr`;\nfor ($xForms in $normalMapGroup)\n{\n    if($xForms == \"normal_map_viewport_render_01\")\n\t{\n\t\tdelete $xForms;\n\t\tstring $maCurrentMayaVersion = `about -product`;\n\t\tif(`gmatch $maCurrentMayaVersion \"*LT*\"`)\n\t\t{\n\t\t\thyperShade -objects normal_map_viewport_render_01_01;\n\t\t\thyperShade -assign phong1;\n\t\t}\n\t\t\n\t\telse\n\t\t{\n\t\t\thyperShade -objects normal_map_viewport_render_01_01;\n\t\t\thyperShade -assign lambert1;\n\t\t}\n\t\tselect normal_map_viewport_render_01_01;\n\t\tdelete;\n\t\tselect -r -ne normal_map_SG_01;\n\t\tdelete;\n\t\t\n\t\tsetFocus modelPanel1;\n\t\tmodelEditor -e -dl \"default\" modelPanel1;\n\t\n\t\tsetFocus modelPanel2;\n\t\tmodelEditor -e -dl \"default\" modelPanel2;\n\t\t\n\t\tsetFocus modelPanel3;\n\t\tmodelEditor -e -dl \"default\" modelPanel3;\n\t\n\t\tsetFocus modelPanel4;\n\t\tmodelEditor -e -dl \"default\" modelPanel4;\n\t\t\n\t\t$normRlSwitch = 1;\n\t\tcolorManagementPrefs -edit -cmEnabled 1;\n\t\tprint \"Normal map render group deleted, colour management turned on.\";\n    }\n}\n\nif($normRlSwitch == 0)\n{\n\tstring $malcolmCreateLight1 = `directionalLight -intensity -1 -name normal_map_01 -position 0 0 32 -rgb 0 1 0`;\n\tsetAttr ($malcolmCreateLight1 + \".emitSpecular\") 0;\n\tsetAttr ($malcolmCreateLight1 + \".useRayTraceShadows\") 0;\n\tsetAttr ($malcolmCreateLight1 + \".useDepthMapShadows\") 0;\n\tsetAttr ($malcolmCreateLight1 + \".emitSpecular\") 0;\n\tsetAttr ($malcolmCreateLight1 + \".useRayTraceShadows\") 0;\n\tsetAttr ($malcolmCreateLight1 + \".useDepthMapShadows\") 0;\n\tscale -r 8 8 8;\n\t$firstNormalLight = `ls -sl`;\n\n\tstring $malcolmCreateLight2 = `directionalLight -intensity 1 -name normal_map_01 -position 0 0 -32 -rgb 0 1 0`;\n\tsetAttr ($malcolmCreateLight2 + \".emitSpecular\") 0;\n\tsetAttr ($malcolmCreateLight2 + \".useRayTraceShadows\") 0;\n\tsetAttr ($malcolmCreateLight2 + \".useDepthMapShadows\") 0;\n\tsetAttr ($malcolmCreateLight2 + \".emitSpecular\") 0;\n\tsetAttr ($malcolmCreateLight2 + \".useRayTraceShadows\") 0;\n\tsetAttr ($malcolmCreateLight2 + \".useDepthMapShadows\") 0;\n\tscale -r 8 8 8;\n\trotate -r -os -fo 0 -180 0;\n\t$secondNormalLight = `ls -sl`;\n\n\tstring $malcolmCreateLight3 = `directionalLight -intensity -1 -name normal_map_01 -position -32 0 0 -rgb 1 0 0`;\n\tsetAttr ($malcolmCreateLight3 + \".emitSpecular\") 0;\n\tsetAttr ($malcolmCreateLight3 + \".useRayTraceShadows\") 0;\n\tsetAttr ($malcolmCreateLight3 + \".useDepthMapShadows\") 0;\n\tsetAttr ($malcolmCreateLight3 + \".emitSpecular\") 0;\n\tsetAttr ($malcolmCreateLight3 + \".useRayTraceShadows\") 0;\n\tsetAttr ($malcolmCreateLight3 + \".useDepthMapShadows\") 0;\n\tscale -r 8 8 8;\n\trotate -r -os -fo 0 -90 0;\n\t$thirdNormalLight = `ls -sl`;\n\n\tstring $malcolmCreateLight4 = `directionalLight -intensity 1 -name normal_map_01 -position 32 0 0 -rgb 1 0 0`;\n\tsetAttr ($malcolmCreateLight4 + \".emitSpecular\") 0;\n\tsetAttr ($malcolmCreateLight4 + \".useRayTraceShadows\") 0;\n\tsetAttr ($malcolmCreateLight4 + \".useDepthMapShadows\") 0;\n\tsetAttr ($malcolmCreateLight4 + \".emitSpecular\") 0;\n\tsetAttr ($malcolmCreateLight4 + \".useRayTraceShadows\") 0;\n\tsetAttr ($malcolmCreateLight4 + \".useDepthMapShadows\") 0;\n\tscale -r 8 8 8;\n\trotate -r -os -fo 0 90 0;\n\t$fourthNormalLight = `ls -sl`;\n\n\tstring $malcolmCreateLight5 = `directionalLight -intensity 1 -name normal_map_01 -position 0 0 0 -rgb 0 0 1`;\n\tsetAttr ($malcolmCreateLight5 + \".emitSpecular\") 0;\n\tsetAttr ($malcolmCreateLight5 + \".useRayTraceShadows\") 0;\n\tsetAttr ($malcolmCreateLight5 + \".useDepthMapShadows\") 0;\n\tsetAttr ($malcolmCreateLight5 + \".emitSpecular\") 0;\n\tsetAttr ($malcolmCreateLight5 + \".useRayTraceShadows\") 0;\n\tsetAttr ($malcolmCreateLight5 + \".useDepthMapShadows\") 0;\n\tscale -r 8 8 8;\n\trotate -r -os -fo -90 0 0;\n\t$fifthNormalLight = `ls -sl`;\n\n\tselect $firstNormalLight;\n\tselect -add $secondNormalLight;\n\tselect -add $thirdNormalLight;\n\tselect -add $fourthNormalLight;\n\tselect -add $fifthNormalLight;\n\n\tgroup -name normal_map_viewport_render_01;\n\n\tcolorManagementPrefs -edit -cmEnabled 0;\n\n\tsetFocus modelPanel1;\n\tmodelEditor -e -displayAppearance \"smoothShaded\" modelPanel1;\n\tDisplayLight;\n\n\tsetFocus modelPanel2;\n\tmodelEditor -e -displayAppearance \"smoothShaded\" modelPanel2;\n\tDisplayLight;\n\n\tsetFocus modelPanel3;\n\tmodelEditor -e -displayAppearance \"smoothShaded\" modelPanel3;\n\tDisplayLight;\n\n\tsetFocus modelPanel4;\n\tmodelEditor -e -displayAppearance \"smoothShaded\" modelPanel4;\n\tDisplayLight;\n\n\t$malcolmLambert = `shadingNode -asShader lambert -name normal_map_viewport_render_01_01`;\n\tsetAttr ($malcolmLambert + \".diffuse\") 1;\n\tsetAttr ($malcolmLambert + \".incandescence\") -type double3 0.5 0.5 0.5;\n\t$normalMapMaterialSelected = `ls -sl`;\n\n\tsets -renderable true -noSurfaceShader true -empty -name normal_map_SG_01;\n\tselect -r -ne normal_map_SG_01;\n\t$normalMapSGSelected = `ls -sl`;\n\n\tconnectAttr -f normal_map_viewport_render_01_01.outColor normal_map_SG_01.surfaceShader;\n\tselect $normalMapMaterialSelected;\n\n//Check if Maya LT if so make a sphere instead of a geosphere\n\tstring $maCurrentMayaVersion = `about -product`;\n\tif(`gmatch $maCurrentMayaVersion \"*LT*\"`)\n\t{\n\t\tpolySphere -r 11 -sx 16 -sy 16 -ax 0 1 0 -cuv 2 -ch 1;\n\t}\n\telse\n\t{\n\t\tpolyPlatonic -primitive 4 -subdivisionMode 1 -subdivisions 2 -radius 11 -sphericalInflation 1;\n\t}\n\trename normal_map_primitive_01;\n\t$selectPlatonic = `ls -sl`;\n\tpolySoftEdge -a 0 -ch 1;\n\tselect $selectPlatonic;\n\tDeleteHistory;\n\n\tparent normal_map_primitive_01 normal_map_viewport_render_01;\n\tsets -e -forceElement normal_map_SG_01;\n\t\n\tselect $normalMapApplySelected;\n\tsets -e -forceElement normal_map_SG_01;\n\n\t\tif($sizeNormLObjs > 0)\n\t\t{\n\t\t\tdelete normal_map_primitive_01;\n\t\t}\n\n\tSelectNone;\t\n    print \"Normal map render group created, colour management turned off.\";\n}\n\n\n\n" 
