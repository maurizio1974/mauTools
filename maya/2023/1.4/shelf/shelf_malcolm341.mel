        -mi "Help normR" ( "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Help\n\nglobal proc m341_HelpNormR()\n{\n\tglobal string $m341_normR_HelpWindow;\n\n\tif (`window -exists $m341_normR_HelpWindow`)\n\t{\n\t\tglobal string $m341_normR_HelpWindow;\n\t\tdeleteUI $m341_normR_HelpWindow;\n\t}\n\t\n\t$m341_normR_HelpWindow = `window -title \"Normal Map Render Help\" -leftEdge 900 -topEdge 115 -toolbox true -sizeable false -resizeToFitChildren false -width 800 -height 230`;\n\tpaneLayout -configuration \"horizontal2\";\n\tscrollField -text \"normR button:\\n\\nClick the button to convert the scene to normal map render mode.\\n\\nClick the button again to convert the scene back into regular render mode.\\n\\nIf you select some objects first and then click the button those objects will have the normal map render material applied to them automatically.\\n\\nNormal maps can now be rendered in the top down camera either in the viewport, hardware renderer, or the Maya software renderer.\\n\\nYoutube video here\\nhttps://youtu.be/JuiIcIRyidE?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\"\n\t-wordWrap true\n\t-enableBackground true\n\t-backgroundColor 0.23 0.23 0.23;\n\t\n\tshowWindow $m341_normR_HelpWindow;\n}\nm341_HelpNormR();" )
