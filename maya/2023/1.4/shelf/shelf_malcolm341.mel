        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Image Ref Z v1.0\n\n//*******************************************************************************************************\n//Image Ref Z\nglobal proc imageRef_Z()\n{\n\t//Check if any file nodes selected, error if not\n\tstring $checkForFilesSelected[] = `ls -sl -exactType file`;\n\t$sizeCheckForFilesSelected = `size $checkForFilesSelected`;\n\tif ($sizeCheckForFilesSelected >= 1)\n\t{\n\t\t//Filter selection for file nodes only\n\t\tstring $firstSelectedRotoNodes[] = `ls -sl -exactType file`;\n\t\t//Check if any file nodes are selected and error if there are none\n\t\tint $rotoCounter = 0;\n\t\t\n\t\tfor ($Item in $firstSelectedRotoNodes)\n\t\t{\n\t\t\t//Select first item in array\n\t\t\tselect $firstSelectedRotoNodes[$rotoCounter];\n\t\t\t//Create file node from selected image and store X/Y resolution of image\n\t\t\t$rotoFileNodeName = `ls -sl`;\n\t\t\t$rotoImageRezX = getAttr ($rotoFileNodeName[0] + \".outSizeX\");\n\t\t\t$rotoImageRezY = getAttr ($rotoFileNodeName[0] + \".outSizeY\");\n\t\t\n\t\t\t//Create surface shader and plug in file node from above\n\t\t\tshadingNode -asShader surfaceShader -name \"m341_rotoscope_MAT\";\n\t\t\t$rotoMaterialNodeName = `ls -sl`;\n\t\t\tconnectAttr -f ($rotoFileNodeName[0] + \".outColor\") ($rotoMaterialNodeName[0] + \".outColor\");\n\t\t\n\t\t\t//Create dummy lambert just so I can find the newly created shading group name because it's not auto selected on creation\n\t\t\tshadingNode -asShader lambert -name \"m341_longUniqueNameDummy_MAT\";\n\t\t\tsets -renderable true -noSurfaceShader true -empty -name \"m341_longUniqueNameDummy_SG\";\n\t\t\tconnectAttr -f m341_longUniqueNameDummy_MAT.outColor m341_longUniqueNameDummy_SG.surfaceShader;\n\t\t\t//Select newly created SG name, rename it, then store it in a variable\n\t\t\tselect -r -ne \"m341_longUniqueNameDummy_SG\";\n\t\t\trename m341_rotoscope_SG;\n\t\t\t$rotoSGNodeName = `ls -sl`;\n\t\t\t//Delete dummy node\n\t\t\tdelete \"m341_longUniqueNameDummy_MAT*\";\n\t\t\n\t\t\t//Plug in shading group to material from above\n\t\t\tconnectAttr -force ($rotoMaterialNodeName[0] + \".outColor\") ($rotoSGNodeName[0] + \".surfaceShader\");\n\t\t\n\t\t\n\t\t\t//Create plane, rotate, freeze, then scale using resolution from image above\n\t\t\tpolyPlane -w 1 -h 1 -sx 1 -sy 1 -ax 0 1 0 -cuv 2 -ch 0 -name (\"IMAGE_\" + $rotoFileNodeName[0]);\n\t\t\tmove -rpr -z -0.5 ;\n\t\t\tmakeIdentity -apply true -t 1 -r 1 -s 1 -n 0 -pn 1;\n\t\t\tmakeIdentity -apply false -t 1 -r 1 -s 1;\n\t\t\trotate -r -os -fo 90 0 0;\n\t\t\tmakeIdentity -apply true -t 1 -r 1 -s 1 -n 0 -pn 1;\n\t\t\t//Store current name and mirror so backface culling works\n\t\t\t$rotoOBJ_1 = `ls -sl`;\n\t\t\tduplicate -rr;\n\t\t\t$rotoOBJ_2 = `ls -sl`;\n\t\t\tpolyNormal -normalMode 0 -userNormalMode 1 -ch 0;\n\t\t\tselect $rotoOBJ_1;\n\t\t\tselect -add $rotoOBJ_2;\n\t\t\tpolyUnite -ch 0 -mergeUVSets 1;\n\t\t\trename $rotoOBJ_1;\n\t\t\t\n\t\t\t//Scale to match resolution of image\n\t\t\tscale -r $rotoImageRezX $rotoImageRezY 0;\n\t\t\n\t\t\t//Scale down to avoid giant image planes\n\t\t\tif ($rotoImageRezY >= 4000)\n\t\t\t{\n\t\t\t\tscale -r 0.05 0.05 0.05;\n\t\t\t}\n\t\t\telse if ($rotoImageRezY >= 3000)\n\t\t\t{\n\t\t\t\tscale -r 0.1 0.1 0.1;\n\t\t\t}\n\t\t\telse if ($rotoImageRezY >= 2000)\n\t\t\t{\n\t\t\t\tscale -r 0.15 0.15 0.15;\n\t\t\t}\n\t\t\telse if ($rotoImageRezY >= 900)\n\t\t\t{\n\t\t\t\tscale -r 0.2 0.2 0.2;\n\t\t\t}\n\t\t\telse if ($rotoImageRezY >= 500)\n\t\t\t{\n\t\t\t\tscale -r 0.45 0.45 0.45;\n\t\t\t}\n\t\t\n\t\t\t//Apply material to plane\n\t\t\tsets -e -forceElement $rotoSGNodeName;\n\t\t\n\t\t\t//Update counter in for loop at top to avoid crash when multiple file nodes selected\n\t\t\t$rotoCounter = $rotoCounter + 1;\n\t\t}\n\t\t\n\t\t//turn on textures for all modeling windows\n\t\tsetFocus modelPanel1;\n\t\tdisplayStyle -textured;\n\t\tsetFocus modelPanel2;\n\t\tdisplayStyle -textured;\n\t\tsetFocus modelPanel3;\n\t\tdisplayStyle -textured;\n\t\tsetFocus modelPanel4;\n\t\tdisplayStyle -textured;\n\t\n\t\t//Select original file node selection for convenience\n\t\tselect $firstSelectedRotoNodes;\n\t\n\t\t//Print success message\n\t\tprint ($rotoCounter + \" image ref objects created on Z\");\n\t}\n\t\n\telse\n\t{\n\t\terror \"Please select at least one file node in the Hypershade\";\n\t}\n\n}\n\n//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//*******************************************************************************************************\n//Image Ref X\nglobal proc imageRef_X()\n{\n\t//Check if any file nodes selected, error if not\n\tstring $checkForFilesSelected[] = `ls -sl -exactType file`;\n\t$sizeCheckForFilesSelected = `size $checkForFilesSelected`;\n\tif ($sizeCheckForFilesSelected >= 1)\n\t{\n\t\t//Filter selection for file nodes only\n\t\tstring $firstSelectedRotoNodes[] = `ls -sl -exactType file`;\n\t\t//Check if any file nodes are selected and error if there are none\n\t\tint $rotoCounter = 0;\n\t\t\n\t\tfor ($Item in $firstSelectedRotoNodes)\n\t\t{\n\t\t\t//Select first item in array\n\t\t\tselect $firstSelectedRotoNodes[$rotoCounter];\n\t\t\t//Create file node from selected image and store X/Y resolution of image\n\t\t\t$rotoFileNodeName = `ls -sl`;\n\t\t\t$rotoImageRezX = getAttr ($rotoFileNodeName[0] + \".outSizeX\");\n\t\t\t$rotoImageRezY = getAttr ($rotoFileNodeName[0] + \".outSizeY\");\n\t\t\n\t\t\t//Create surface shader and plug in file node from above\n\t\t\tshadingNode -asShader surfaceShader -name \"m341_rotoscope_MAT\";\n\t\t\t$rotoMaterialNodeName = `ls -sl`;\n\t\t\tconnectAttr -f ($rotoFileNodeName[0] + \".outColor\") ($rotoMaterialNodeName[0] + \".outColor\");\n\t\t\n\t\t\t//Create dummy lambert just so I can find the newly created shading group name because it's not auto selected on creation\n\t\t\tshadingNode -asShader lambert -name \"m341_longUniqueNameDummy_MAT\";\n\t\t\tsets -renderable true -noSurfaceShader true -empty -name \"m341_longUniqueNameDummy_SG\";\n\t\t\tconnectAttr -f m341_longUniqueNameDummy_MAT.outColor m341_longUniqueNameDummy_SG.surfaceShader;\n\t\t\t//Select newly created SG name, rename it, then store it in a variable\n\t\t\tselect -r -ne \"m341_longUniqueNameDummy_SG\";\n\t\t\trename m341_rotoscope_SG;\n\t\t\t$rotoSGNodeName = `ls -sl`;\n\t\t\t//Delete dummy node\n\t\t\tdelete \"m341_longUniqueNameDummy_MAT*\";\n\t\t\n\t\t\t//Plug in shading group to material from above\n\t\t\tconnectAttr -force ($rotoMaterialNodeName[0] + \".outColor\") ($rotoSGNodeName[0] + \".surfaceShader\");\n\t\t\n\t\t\n\t\t\t//Create plane, rotate, freeze, then scale using resolution from image above\n\t\t\tpolyPlane -w 1 -h 1 -sx 1 -sy 1 -ax 0 1 0 -cuv 2 -ch 0 -name (\"IMAGE_\" + $rotoFileNodeName[0]);\n\t\t\tmove -rpr -z -0.5 ;\n\t\t\tmakeIdentity -apply true -t 1 -r 1 -s 1 -n 0 -pn 1;\n\t\t\tmakeIdentity -apply false -t 1 -r 1 -s 1;\n\t\t\trotate -r -os -fo 90 0 0;\n\t\t\t//Rotate for X\n\t\t\trotate -r -os -fo 0 0 90;\n\t\t\tmakeIdentity -apply true -t 1 -r 1 -s 1 -n 0 -pn 1;\n\t\t\t//Store current name and mirror so backface culling works\n\t\t\t$rotoOBJ_1 = `ls -sl`;\n\t\t\tduplicate -rr;\n\t\t\t$rotoOBJ_2 = `ls -sl`;\n\t\t\tpolyNormal -normalMode 0 -userNormalMode 1 -ch 0;\n\t\t\tselect $rotoOBJ_1;\n\t\t\tselect -add $rotoOBJ_2;\n\t\t\tpolyUnite -ch 0 -mergeUVSets 1;\n\t\t\trename $rotoOBJ_1;\n\t\t\t\n\t\t\t//Scale to match resolution of image\n\t\t\tscale -r 0 $rotoImageRezY $rotoImageRezX;\n\t\t\n\t\t\t//Scale down to av