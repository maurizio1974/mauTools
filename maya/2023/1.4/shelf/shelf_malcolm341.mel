apperSelectedUVs;\n\tprint \"Unfold UV complete\";\n}\n\n//***********************************************************************************************************************************************************\n//Straighten UV angle 30\nglobal proc button11UVmapper ()\n{\n\t//Error 3D cut and sew\n\tstring $currentActiveTool = `currentCtx`;\n\tif(`gmatch $currentActiveTool \"*superCutUVContext*\"`)\n\t{\n\t\terror \"Please exit 3d cut and sew\";\n\t} \n\tif(`gmatch $currentActiveTool \"*PolyCutUVCtx*\"`)\n\t{\n\t\terror \"Please exit 3d cut and sew\";\n\t}\t\n\n\t//Check if select mode is faces or shells\n\t$component_Face = `selectType -q -facet`;\n\t$component_Shell = `selectType -q -meshUVShell`;\n\t$component_Vert = `selectType -q -vertex`;\n\t$component_Edge = `selectType -q -edge`;\n\n\t$maUVMapperSelectedUVs = `ls -sl`;\n\ttexStraightenUVs \"UV\" 30;\n\ttoggleSelMode;\n\ttoggleSelMode;\n\tselectMode -object;\n\tDeleteHistory;\n\tConvertSelectionToUVs;\n\t\t\n\t//Bring back original component mode user was in\t\t\n\tif ($component_Shell > 0)\n\t{\n\t\tcomponentMode2_Shells();\n\t}\n\t\n\tif ($component_Face > 0)\n\t{\n\t\tcomponentMode2_Faces();\n\t}\n\t\n\tif ($component_Vert > 0)\n\t{\n\t\tcomponentMode2_Verts();\n\t}\n\t\n\tif ($component_Edge > 0)\n\t{\n\t\tcomponentMode2_Edges();\n\t}\n\t\t\n\tselect $maUVMapperSelectedUVs;\n\tprint \"Straighten UV complete\";\n}\n\n//***********************************************************************************************************************************************************\n//Straighten UV shell\nglobal proc button17UVmapper ()\n{\n\t//Error 3D cut and sew\n\tstring $currentActiveTool = `currentCtx`;\n\tif(`gmatch $currentActiveTool \"*superCutUVContext*\"`)\n\t{\n\t\terror \"Please exit 3d cut and sew\";\n\t} \n\tif(`gmatch $currentActiveTool \"*PolyCutUVCtx*\"`)\n\t{\n\t\terror \"Please exit 3d cut and sew\";\n\t}\t\n\n\t$edges = `ls -sl`;\n\ttexStraightenShell;\n\ttoggleSelMode;\n\ttoggleSelMode;\n\