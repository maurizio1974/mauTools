        -mi "Find File Location" ( "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Find Temp Folder v1.0\n\n{\n\tstring $maCurrentMayaVersionMac = `about -mac`;\n\tif($maCurrentMayaVersionMac == 1)\n\t{\n\t\tsysFile -makeDir \"/Users/Shared/matemp\";\n\t\tlaunch -directory \"/Users/Shared/matemp\";\n \t}\t\n\telse\n\t{\n\t\tsysFile -makeDir \"C:/matemp\";\n\t\tlaunch -directory \"C:/matemp\";\n\t}\t\n}" )
        -mi "Help import" ( "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Help\n\nglobal proc m341_HelpImportExport()\n{\n\tglobal string $m341_ImportExport_HelpWindow;\n\n\tif (`window -exists $m341_ImportExport_HelpWindow`)\n\t{\n\t\tglobal string $m341_ImportExport_HelpWindow;\n\t\tdeleteUI $m341_ImportExport_HelpWindow;\n\t}\n\t\n\t$m341_ImportExport_HelpWindow = `window -title \"Import Export Help\" -leftEdge 900 -topEdge 115 -toolbox true -sizeable false -resizeToFitChildren false -width 800 -height 380`;\n\tpaneLayout -configuration \"horizontal2\";\n\tscrollField -text \"import/export buttons:\\n\\nSelect some objects and click the export button.\\n\\nThe selected objects will be stored on your hard drive as a temp.ma file.\\n\\nOpen a different Maya scene and click the import button.\\n\\nThe selected objects from the previous step will be imported into your currently open scene.\\n\\nThis can be very useful for quickly grabbing just the objects you want and creating a clean scene without doing any manual file management.\\n\\nYou can even have two copies of Maya open at the same time and copy paste between the two by clicking the export button in one Maya and click the import button in the other Maya.\\n\\nThe temp file will remain on your hard drive until you overwrite it with the next export, even if you close Maya.\\n\\nIf you right click the buttons you can import/export a temp .obj file instead or browse to the file export locaton.\\n\\nYoutube video here\\nhttps://youtu.be/mydwcKHeFFo?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\"\n\t-wordWrap true\n\t-enableBackground true\n\t-backgroundColor 0.23 0.23 0.23;\n\t\n\tshowWindow $m341_ImportExport_HelpWindow;\n}\nm341_HelpImportExport();" )
