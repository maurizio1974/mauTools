        -mi "8192 x 8192" ( "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\nstring $uvGrabberVersion = \"uvGrabber v1.6\";\n\n//Check if Maya is running on the Mac and adjust file paths appropriately\nstring $maCurrentMayaVersionMac = `about -mac`;\nglobal string $uvGrab_DirPath;\nglobal string $uvGrab_ImgPath;\nif($maCurrentMayaVersionMac == 1)\n{\n\tglobal string $uvGrab_DirPath;\n\tglobal string $uvGrab_ImgPath;\n\t$uvGrab_DirPath = \"/Users/Shared/matemp\";\n\t$uvGrab_ImgPath = \"/Users/Shared/matemp/uvGrab_U1_V1.png\";\n}\nelse\n{\n\tglobal string $uvGrab_DirPath;\n\tglobal string $uvGrab_ImgPath;\n    $uvGrab_DirPath = \"C:/matemp\";\n\t$uvGrab_ImgPath = \"C:/matemp/uvGrab_U1_V1.png\";\n}\n\nsysFile -makeDir $uvGrab_DirPath;\nstring $maUVSnapShotSelected[] = `ls -sl`;\nfloat $maSnapNumbObjs = size($maUVSnapShotSelected);\n\nif ($maSnapNumbObjs >= 1)\n{\n\t//Clean folder if old images exist\n\tglobal string $m341_uvGrab_DirPath;\n\tstring $queryFolderFirst[] = `getFileList -folder $m341_uvGrab_DirPath -filespec \"uvGrab*\"`;\n\tint $queryFolderFirstSize = `size $queryFolderFirst`;\n\tint $delCounter = 0;\n\tfor ($ItemWire = 0; $ItemWire < $queryFolderFirstSize; $ItemWire++)\n\t{\n\t\tsysFile -delete ($m341_uvGrab_DirPath + \"/\" + $queryFolderFirst[$delCounter]);\n\t\t$delCounter = $delCounter + 1;\n\t}\n\n\tuvSnapshot -o -ff png -xr 8192 -yr 8192 -r 0 -g 0 -b 0 -n $uvGrab_ImgPath -uMin 0 -uMax 1 -vMin 0 -vMax 1;\n\timportImageFile ($uvGrab_ImgPath, false, false, false);\n\tEditTexture;\n\tdelete;\n\tselect $maUVSnapShotSelected;\n\tprint \"UV snapshot sent to image editor\";\n}\n\nelse\n{\n\terror \"Select at least one object first\";\n}\t" )
