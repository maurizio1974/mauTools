 0 -pn 1;\n\t\t\tmakeIdentity -apply false -t 1 -r 1 -s 1;\n\t\n\t\t\t//Store current name and mirror so backface culling works\n\t\t\t$rotoOBJ_1 = `ls -sl`;\n\t\t\tduplicate -rr;\n\t\t\t$rotoOBJ_2 = `ls -sl`;\n\t\t\tpolyNormal -normalMode 0 -userNormalMode 1 -ch 0;\n\t\t\tselect $rotoOBJ_1;\n\t\t\tselect -add $rotoOBJ_2;\n\t\t\tpolyUnite -ch 0 -mergeUVSets 1;\n\t\t\trename $rotoOBJ_1;\n\t\t\t\n\t\t\t//Scale to match resolution of image\n\t\t\tscale -r $rotoImageRezX 0 $rotoImageRezY;\n\t\t\n\t\t\t//Scale down to avoid giant image planes\n\t\t\tif ($rotoImageRezY >= 4000)\n\t\t\t{\n\t\t\t\tscale -r 0.05 0.05 0.05;\n\t\t\t}\n\t\t\telse if ($rotoImageRezY >= 3000)\n\t\t\t{\n\t\t\t\tscale -r 0.1 0.1 0.1;\n\t\t\t}\n\t\t\telse if ($rotoImageRezY >= 2000)\n\t\t\t{\n\t\t\t\tscale -r 0.15 0.15 0.15;\n\t\t\t}\n\t\t\telse if ($rotoImageRezY >= 900)\n\t\t\t{\n\t\t\t\tscale -r 0.2 0.2 0.2;\n\t\t\t}\n\t\t\telse if ($rotoImageRezY >= 500)\n\t\t\t{\n\t\t\t\tscale -r 0.45 0.45 0.45;\n\t\t\t}\n\t\t\n\t\t\t//Apply material to plane\n\t\t\tsets -e -forceElement $rotoSGNodeName;\n\t\t\n\t\t\t//Update counter in for loop at top to avoid crash when multiple file nodes selected\n\t\t\t$rotoCounter = $rotoCounter + 1;\n\t\t}\n\t\t\n\t\t//turn on textures for all modeling windows\n\t\tsetFocus modelPanel1;\n\t\tdisplayStyle -textured;\n\t\tsetFocus modelPanel2;\n\t\tdisplayStyle -textured;\n\t\tsetFocus modelPanel3;\n\t\tdisplayStyle -textured;\n\t\tsetFocus modelPanel4;\n\t\tdisplayStyle -textured;\n\t\n\t\t//Select original file node selection for convenience\n\t\tselect $firstSelectedRotoNodes;\n\t\n\t\t//Print success message\n\t\tprint ($rotoCounter + \" image ref objects created on Y\");\n\t}\n\t\n\telse\n\t{\n\t\terror \"Please select at least one file node in the Hypershade\";\n\t}\n}\n\nimageRef_X();\nimageRef_Y();\nimageRef_Z();\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n" )
        -mi "Help imgRef" ( "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Help\n\nglobal proc m341_HelpImgRef()\n{\n\tglobal string $m341_imgRef_HelpWindow;\n\n\tif (`window -exists $m341_imgRef_HelpWindow`)\n\t{\n\t\tglobal string $m341_imgRef_HelpWindow;\n\t\tdeleteUI $m341_imgRef_HelpWindow;\n\t}\n\n\t$m341_imgRef_HelpWindow = `window -title \"Reference Planes Help\" -leftEdge 900 -topEdge 115 -toolbox true -sizeable false -resizeToFitChildren false -width 800 -height 200`;\n\tpaneLayout -configuration \"horizontal2\";\n\tscrollField -text \"imgRef button:\\n\\nSelect one or more file nodes in the Hypershade and click the imgRef button.\\n\\nAn image reference object is created in the scene for each file node, correct width/height is applied to prevent the texture from skewing.\\n\\nReference objects are created facing the Z axis, right click the imgRef button on the shelf and you can choose the X, Y, or XYZ axis as well.\\n\\nYoutube video here\\nhttps://youtu.be/93yN2lFWkTo?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\"\n\t-wordWrap true\n\t-enableBackground true\n\t-backgroundColor 0.23 0.23 0.23;\n\t\n\tshowWindow $m341_imgRef_HelpWindow;\n}\nm341_HelpImgRef();" )
