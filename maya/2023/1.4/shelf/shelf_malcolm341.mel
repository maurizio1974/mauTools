        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\nstring $uvGrabberVersion = \"uvGrabber v1.6\";\n\n//***********************************************************************************************************************************************************\n//* global variables\n//***********************************************************************************************************************************************************\n\n//***********************************************************************************************************************************************************\n//Help\nglobal proc helpmaUVSnapShot()\n{\n\tglobal string $m341_uvGrabber_HelpWindow;\n\n\tif (`window -exists $m341_uvGrabber_HelpWindow`)\n\t{\n\t\tglobal string $m341_uvGrabber_HelpWindow;\n\t\tdeleteUI $m341_uvGrabber_HelpWindow;\n\t}\n\t\n\t$m341_uvGrabber_HelpWindow = `window -title \"uvGrabber Help\" -leftEdge 900 -topEdge 115 -toolbox true -sizeable false -resizeToFitChildren false -width 800 -height 470`;\n\t\t\n\tpaneLayout -configuration \"horizontal2\";\n\tscrollField -text \"Change the X/Y size to whatever you want.\\n\\nSelect some objects and click the snapshot button, the UVs for those objects will be opened in Maya's currently assigned image editing program. By default this is fcheck, but you can change this to whatever you like.\\n\\nClick the find button to open the folder where the UV snapshots are stored.\\nPC file location C:\\\\matemp\\nMac file location /Users/Shared/matemp\\n\\nYou can also right click the uvGrab shelf button and choose from some common resolutions to save time without having to open the toolbox.\\n\\nIf you want the UVs to open in Photoshop instead of fcheck, in Maya go to:\\n\\nWindows> Settings/Preferences> Applications> Image Editing Applications, then under image editor click the folder icon and browse to the program you want to use and select it, in this case Photoshop.\\n\\nIf you don't want the UVs to automatically open in the assigned image editor uncheck the send to editor checkbox. This can be handy for capturing a lot of UDIM tiles because the files will generate faster if they don't need to be sent to the image editor.\\n\\nIf you want to capture the UVs for multiple UDIM tiles adjust the tiles U and tiles V.\\n\\nIf you want to choose a different UV set, select an object and click the refresh button to load that object's UV list, then select the desired UV set from the drop down. Different UV sets can be exported even if the UV editor is closed.\\n\\nYoutube videos here\\nhttps://youtu.be/wFpu3WfVSBw?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\\nhttps://youtu.be/rq6JP3HoV0g?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\"\n\t-wordWrap true\n\t-enableBackground true\n\t-backgroundColor 0.23 0.23 0.23;\n\t\n\tshowWindow $m341_uvGrabber_HelpWindow;\n}\n\n\n//***********************************************************************************************************************************************************\n//Kill Script Job\nglobal proc killScriptJob ()\n{\n\tglobal int $m341_refreshUVList_ScriptJob;\n\tscriptJob -kill $m341_refreshUVList_ScriptJob -force;\t\t\n}\n\n//***********************************************************************************************************************************************************\n//Refresh UV List Script Job\nglobal proc m341_refreshUVList_ScriptJobProc()\n{\n\tstring $uvSet_All[] = `polyUVSet -q -allUVSets`;\n\tint $uvSet_AllSize = `size $uvSet_All`;\n\n\tglobal string $m341_objNameGrabber1[];\n\tglobal string $m341_objNameGrabber2[];\n\t\n\t$m341_objNameGrabber1 = `ls -o -sl`;\n\t\t\n\tif ($m341_objNameGrabber1[0] == $m341_objNameGrabber2[0])\n\t{\n\t\t//Select correct list item after drop down was refreshed\n\t\tglobal int $m341_currentUVSet;\n\t\t//optionMenu -edit -select $m341_currentUVSet -enable true uvSetDropDown;\n\t\toptionMenu -edit -enable true uvSetDropDown;\n\t\tprint \"Same object found\";\n\t}\n\t\n\telse\n\t{\n\t\tif ($uvSet_AllSize > 0)\n\t\t{\n\t\t\t//Query current UV set\n\t\t\tglobal int $m341_currentUVSet;\n\t\t\t$m341_currentUVSet = `optionMenu -q -select uvSetDropDown`;\n\t\t\t\n\t\t\t//Query menu items in UV set drop down and delete them\n\t\t\tstring $uvSetMenuItems[] = `optionMenu -q -itemListLong uvSetDropDown`;\n\t\t\tdeleteUI $uvSetMenuItems;\n\t\t\t\n\t\t\t//Add refreshed menu items from new object\n\t\t\tstring $uvSet_All[] = `polyUVSet -q -allUVSets`;\n\t\t\tint $uvSet_AllSize = `size $uvSet_All`;\n\t\t\tint $menuItemCounter = 0;\n\t\n\t\t\tfor ($Item = 0; $Item < $uvSet_AllSize; $Item++)\n\t\t\t{\n\t\t\t\tmenuItem -parent uvSetDropDown -label $uvSet_All[$menuItemCounter];\n\t\t\t\t$menuItemCounter = $menuItemCounter + 1;\n\t\t\t}\n\t\t\n\t\t\t//Select first list item after drop down was refreshed\n\t\t\toptionMenu -edit -select 1 -enable true uvSetDropDown;\n\t\t\t//Set UV set to map1\n\t\t\tpolyUVSet -currentUVSet -uvSet $uvSet_All[0];\n\t\t\t\n\t\t\t\n\t\t\t//Check if UV set name exists so UV set list doesn't reset when selecting components\n\t\t\t//string $yourArray[] = `polyUVSet -q -allUVSets`;\n\n\t\t\t//for ($Item in $yourArray)\n\t\t\t//{\n\t\t\t\t//if (`gmatch $Item $uvSet_All[0]`)\n\t\t\t\t//{\n\t\t\t\t//\toptionMenu -edit -select $m341_currentUVSet -enable true uvSetDropDown;\n\t\t\t\t//polyCone -r 1 -h 2 -sx 20 -sy 1 -sz 0 -ax 0 1 0 -rcp 0 -cuv 3 -ch 1; objectMoveCommand;\n\t\t\t\t//}\n\t\t\t//}\n\t\t\t\n\t\t\tglobal string $m341_objNameGrabber1[];\n\t\t\tglobal string $m341_objNameGrabber2[];\n\t\t\t$m341_objNameGrabber2 = $m341_objNameGrabber1;\n\t\t\t\n\t\t\tprint \"New object found\";\n\t\t}\n\t\t\n\t\telse\n\t\t{\n\t\t\t//If something selected that doesn't have UV sets grey out drop down list\n\t\t\toptionMenu -edit -enable false uvSetDropDown;\n\t\t\tprint \"No UV sets found\";\n\t\t}\t\n\t}\n}\n\n//***********************************************************************************************************************************************************\n//Change Min U value\nglobal proc minU_ValueChanged()\n{\n\tglobal string $m341_minU_IntFieldName;\n\tglobal int $m341_minU_userInput;\t\t\t\n\t//Query the value from the float field and store it in a variable.\n\t$m341_minU_userInput = `intField -query -value $m341_minU_IntFieldName`;\n\tprint(\"Min U range set to \"+ $m341_minU_userInput);\n}\n\n//***********************************************************************************************************************************************************\n//Refresh UV List Button\nglobal proc refreshUVList()\n{\n\t\tstring $uvSet_All[] = `polyUVSet -q -allUVSets`;\n\tint $uvSet_AllSize = `size $uvSet_All`;\n\t\n\tif ($uvSet_AllSize > 0)\n\t{\n\t\t//Query menu items in UV set drop down and delete them\n\t\tstring $uvSetMenuItems[] = `optionMenu -q -itemListLong uvSetDropDown`;\n\t\tdeleteUI $uvSetMenuItems;\n\t\n\t\t//Add refreshed menu items from new object\n\t\tstring $uvSet_All[] = `polyUVSet -q -allUVSets`;\n\t\tint $uvSet_AllSize = `size $uvSet_All`;\n\t\tint $menuItemCounter = 0;\n\t\t\n\t\tfor ($Item = 0; $Item < $uvSet_AllSize; $Item++)\n\t\t{\n\t\t\tmenuItem -parent uvSetDropDown -label $uvSet_All[$menuItemCounter];\n\t\t\t$menuItemCounter = $menuItemCounter + 1;\n\t\t}\n\t\toptionMenu -edit -select 1 -enable true uvSetDropDown;\n\t\tpolyUVSet -currentUVSet -uvSet $uvSet_All[0];\n\t\tprint \"UV list refreshed\";\n\t}\n\t\n\telse\n\t{\n\t\toptionMenu -edit -select 1 -enable false uvSetDropDown;\n\t\tprint \"No UV sets found, select something with UVs and click refresh\";\n\t}\n}\n\n//Change Int Value UDIM U\nglobal proc intValueChanged_UDIM_U()\n{\n\tglobal string $m341_intFieldName_UDIM_U;\n\tglobal int $m341_userInput_UDIM_U;\t\t\t\n\t// query the value from the int field and store it in a variable\n\t$m341_userInput_UDIM_U = `intField -query -value $m341_intFieldName_UDIM_U`;\n\t// print value\n\tprint(\"UV tiles U set to \"+ $m341_userInput_UDIM_U +\"\\n\");\n}\n\n//Change Int Value UDIM U\nglobal proc intValueChanged_UDIM_V()\n{\n\tglobal string $m341_intFieldName_UDIM_V;\n\tglobal int $m341_userInput_UDIM_V;\t\t\t\n\t// query the value from the int field and store it in a variable\n\t$m341_userInput_UDIM_V = `intField -query -value $m341_intFieldName_UDIM_V`;\n\t// print value\n\tprint(\"UV tiles V set to \"+ $m341_userInput_UDIM_V +\"\\n\");\n}\n\n//***********************************************************************************************************************************************************\n//UV Snapshot\nglobal proc button1maUVSnapShot ()\n{\n\tint $uvResolutionXIndex = `optionMenu -query -select uvResolutionX`;\n\tfloat $uvResolutionXArray[] = {8192, 4096, 2048, 1024, 512, 256, 128, 64, 32};\n\tint $uvResolutionYIndex = `optionMenu -query -select uvResolutionY`;\n\tfloat $uvResolutionYArray[] = {8192, 4096, 2048, 1024, 512, 256, 128, 64, 32};\n\n\t//Query UI current tiles U set by user\n\tintValueChanged_UDIM_U();\n\tglobal string $m341_intFieldName_UDIM_U;\n\tglobal int $m341_userInput_UDIM_U;\t\t\n\t\n\t//Query UI current tiles V set by user\n\tintValueChanged_UDIM_V();\n\tglobal string $m341_intFieldName_UDIM_V;\n\tglobal int $m341_userInput_UDIM_V;\n\t\n\tglobal string $m341_uvGrab_DirPath;\n\t\n\t//Check if Maya is running on the Mac and create folder on hard drive using the PC or Mac location\n\tstring $maCurrentMayaVersionMac = `about -mac`;\n\tif($maCurrentMayaVersionMac == 1)\n\t{\n\t\tglobal string $m341_uvGrab_DirPath;\n\t\t$m341_uvGrab_DirPath = \"/Users/Shared/matemp\";\n\t\tsysFile -makeDir $m341_uvGrab_DirPath;\n\t}\n\telse\n\t{\n\t\tglobal string $m341_uvGrab_DirPath;\n\t\t$m341_uvGrab_DirPath = \"C:/matemp\";\n\t\tsysFile -makeDir $m341_uvGrab_DirPath;\n\t}\n\t\n\t//Store object name to select it later\n\tstring $maUVSnapShotSelected[] = `ls -sl`;\n\tfloat $maSnapNumbObjs = size($maUVSnapShotSelected);\n\t\n\t//Check if at least one object selected error if not down below\n\tif ($maSnapNumbObjs >= 1)\n\t{\n\t\n\t\t//Clean folder if old images exist\n\t\tglobal string $m341_uvGrab_DirPath;\n\t\tstring $queryFolderFirst[] = `getFileList -folder $m341_uvGrab_DirPath -filespec \"uvGrab*\"`;\n\t\tint $queryFolderFirstSize = `size $queryFolderFirst`;\n\t\tint $delCounter = 0;\n\t\tfor ($ItemWire = 0; $ItemWire < $queryFolderFirstSize; $ItemWire++)\n\t\t{\n\t\t\t//print ($queryFolder[$counter] + \"\\n\");\n\t\t\tsysFile -delete ($m341_uvGrab_DirPath + \"/\" + $queryFolderFirst[$delCounter]);\n\t\t\t$delCounter = $delCounter + 1;\n\t\t}\n\t\t\n\t\t//If only one U and V tile do a faster export of the wireframe without pause\n\t\tif ($m341_userInput_UDIM_U == 1 && $m341_userInput_UDIM_U == 1)\n\t\t{\n\t\t\t//If Mac change path\n\t\t\tstring $maCurrentMayaVersionMac = `about -mac`;\n\t\t\tif($maCurrentMayaVersionMac == 1)\n\t\t\t{\n\t\t\t\tglobal string $m341_uvGrab_FinalPath;\n\t\t\t\t$m341_uvGrab_FinalPath = \"/Users/Shared/matemp/uvGrab_U1_V1.png\";\n\t\t\t}\n\t\t\telse\n\t\t\t{\n\t\t\t\tglobal string $m341_uvGrab_FinalPath;\n\t\t\t\t$m341_uvGrab_FinalPath = \"C:/matemp/uvGrab_U1_V1.png\";\n\t\t\t}\n\t\t\t\n\t\t\tglobal string $m341_uvGrab_FinalPath;\n\t\t\tuvSnapshot -overwrite -ff png -xr $uvResolutionXArray[$uvResolutionXIndex -1] -yr $uvResolutionYArray[$uvResolutionYIndex -1] -r 0 -g 0 -b 0 -n $m341_uvGrab_FinalPath -uMin 0 -uMax 1 -vMin 0 -vMax 1;\n\t\t\t\n\t\t\tint $openImages_CheckMarkValue = `checkBox -query -value openImages_CheckBox`;\n\t\t\tif ($openImages_CheckMarkValue == 1)\n\t\t\t{\n\t\t\t\t//Open the files in Photoshop\n\t\t\t\timportImageFile ($m341_uvGrab_FinalPath, false, false, false);\n\t\t\t\tEditTexture;\n\t\t\t}\n\t\t}\n\t\t\n\t\telse\n\t\t{\n\t\t\n\t\t\tint $uMin = 0.0;\n\t\t\tint $uMax = 1.0;\n\t\t\tint $vMin = 0.0;\n\t\t\tint $vMax = 1.0;\n\t\n\t\t\tfor ($baseV = 0; $baseV < $m341_userInput_UDIM_V; $baseV++)\n\t\t\t{\n\t\t\t\tfor ($baseU = 0; $baseU < $m341_userInput_UDIM_U; $baseU++)\n\t\t\t\t{\n\t\t\t\t\t//Add one to the name\n\t\t\t\t\tint $labelU = $baseU + 1;\n\t\t\t\t\tint $labelV = $baseV + 1;\n\t\t\t\t\t\n\t\t\t\t\t//If Mac change path\n\t\t\t\t\tstring $maCurrentMayaVersionMac = `about -mac`;\n\t\t\t\t\tif($maCurrentMayaVersionMac == 1)\n\t\t\t\t\t{\n\t\t\t\t\t\tglobal string $m341_uvGrab_FinalPath;\n\t\t\t\t\t\t$m341_uvGrab_FinalPath = (\"/Users/Shared/matemp/uvGrab_U\" + $labelU + \"_V\" + $labelV + \".png\");\n\t\t\t\t\t}\n\t\t\t\t\telse\n\t\t\t\t\t{\n\t\t\t\t\t\tglobal string $m341_uvGrab_FinalPath;\n\t\t\t\t\t\t$m341_uvGrab_FinalPath = (\"C:/matemp/uvGrab_U\" + $labelU + \"_V\" + $labelV + \".png\");\n\t\t\t\t\t}\n\t\t\t\t\t\n\t\t\t\t\t//Add one to offset the UV tiles before snapshot\n\t\t\t\t\t$uMin = $baseU;\n\t\t\t\t\t$uMax = $baseU + 1.0;\n\t\t\t\t\t$vMin = $baseV;\n\t\t\t\t\t$vMax = $baseV + 1.0;\n\t\t\t\t\tglobal string $m341_uvGrab_FinalPath;\n\t\t\t\t\tuvSnapshot -overwrite -ff png -xr $uvResolutionXArray[$uvResolutionXIndex -1] -yr $uvResolutionYArray[$uvResolutionYIndex -1] -r 0 -g 0 -b 0 -n $m341_uvGrab_FinalPath -uMin $uMin -uMax $uMax -vMin $vMin -vMax $vMax;\n\t\t\t\t\t\n\t\t\t\t\tint $openImages_CheckMarkValue = `checkBox -query -value openImages_CheckBox`;\n\t\t\t\t\tif ($openImages_CheckMarkValue == 1)\n\t\t\t\t\t{\n\t\t\t\t\t\t//Pause to fix bug where sometimes it doesn't open all the files\n\t\t\t\t\t\tpause -seconds 1; \n\t\t\t\t\t\t//Open the files in Photoshop\n\t\t\t\t\t\timportImageFile ($m341_uvGrab_FinalPath, false, false, false);\n\t\t\t\t\t\t//dR_updateToolSettings;\n\t\t\t\t\t\tEditTexture;\n\t\t\t\t\t}\n\t\t\t\t\tselect $maUVSnapShotSelected;\n\t\t\t\t}\n\t\t\t}\n\t\t}\n\t\t\n\t\tselect $maUVSnapShotSelected;\n\t}\n\t\n\telse\n\t{\n\t\terror \"Select at least one object first\";\n\t}\t\n\t\nprint \"UV snapshots sent to image editor\";\n}\n\n//Find Folder Location\nglobal proc buttonFindUVSnapShot()\n{\n\tstring $maCurrentMayaVersionMac = `about -mac`;\n\tif($maCurrentMayaVersionMac == 1)\n\t{\n\t\tsysFile -makeDir \"/Users/Shared/matemp\";\n\t\tlaunch -directory \"/Users/Shared/matemp\";\n \t}\t\n\telse\n\t{\n\t\tsysFile -makeDir \"C:/matemp\";\n\t\tlaunch -directory \"C:/matemp\";\n\t}\t\n}\n//***********************************************************************************************************************************************************\n//* window and buttons\n//***********************************************************************************************************************************************************\nif (`window -exists maUVSnapShot`)\n{\n\tdeleteUI maUVSnapShot;\n}\n\n{\n\t//Turn on script job so UV list updates on selection\n\t//global int $m341_refreshUVList_ScriptJob;\n\t//$m341_refreshUVList_ScriptJob = `scriptJob -compressUndo 1 -runOnce 0 -event \"SelectionChanged\" \"m341_refreshUVList_ScriptJobProc\"`;\n\n\tstring $uvSet_All[] = `polyUVSet -q -allUVSets`;\n\t\n\twindow -title $uvGrabberVersion -toolbox true -sizeable false -resizeToFitChildren true maUVSnapShot;\n\tcolumnLayout -adjustableColumn true;\n\ttext -label \"by malcolm341\";\n\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 1 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 50 -columnWidth 2 30;\n\ticonTextButton -style \"textOnly\"\n\t\t//-font \"smallPlainLabelFont\"\t\n\t\t-labelOffset 30\n\t\t-label \"click here for help\"\n\t\t-backgroundColor 0.51 0.51 0.51\n\t\t-width 117\n\t\t-height 15\n\t\t-command \"helpmaUVSnapShot;\"\n\t\t-commandRepeatable 1;\n\t\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 2 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 67 -columnWidth 2 30;\n\t\tcheckBox -value 1 -label \"send to editor\" openImages_CheckBox;\n\t\t\n\tsetParent ..;\n\trowLayout -numberOfColumns 2 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 37 -columnWidth 2 30;\n\t\tsetParent ..;\n\t\tsetParent ..;\n\t\trowLayout -numberOfColumns 2 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 37 -columnWidth 2 30;\n\t\t\toptionMenu -w 112 -label \"size X \" uvResolutionX;\n\t\t\tmenuItem -label \"8192 pix\";\n\t\t\tmenuItem -label \"4096 pix\";\n\t\t\tmenuItem -label \"2048 pix\";\n\t\t\tmenuItem -label \"1024 pix\";\n\t\t\tmenuItem -label \"512 pix\";\n\t\t\tmenuItem -label \"256 pix\";\n\t\t\tmenuItem -label \"128 pix\";\n\t\t\tmenuItem -label \"64 pix\";\n\t\t\tmenuItem -label \"32 pix\";\n\t\t\toptionMenu -edit -select 3 uvResolutionX;\n\t\t\t\n\t\tsetParent ..;\n\t\tsetParent ..;\n\t\trowLayout -numberOfColumns 2 -columnAttach2 \"left\" \"left\" -columnOffset2 5 0 -columnWidth 1 37 -columnWidth 2 30;\n\t\t\toptionMenu -w 112 -label \"size Y \" uvResolutionY;\n\t\t\tmenuItem -label \"8192 pix\";\n\t\t\tmenuItem -label \"4096 pix\";\n\t\t\tmenuItem -label \"2048 pix\";\n\t\t\tmenuItem -label \"1024 pix\";\n\t\t\tmenuItem -label \"512 pix\";\n\t\t\tmenuItem -label \"256 pix\";\n\t\t\tmenuItem -label \"128 pix\";\n\t\t\tmenuItem -label \"64 pix\";\n\t\t\tmenuItem -label \"32 pix\";\n\t\t\toptionMenu -edit -select 3 uvResolutionY;\n\t\t\t\n\t\tsetParent ..;\n\t\trowColumnLayout -numberOfRows 1;\n\t\ttext \"  tiles U \";\n\t\t$m341_intFieldName_UDIM_U = `intField -value 1 -minValue 1 -width 30 -changeCommand \"intValueChanged_UDIM_U();\"`;\n\t\ttext \"  V \";\n\t\t$m341_intFieldName_UDIM_V = `intField -value 1 -minValue 1 -width 30 -changeCommand \"intValueChanged_UDIM_V();\"`;\n\t\t\n\t\t\t\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\ticonTextButton -style \"textOnly\"\n\t\t-labelOffset 30\n\t\t-label \"refresh\"\n\t\t-backgroundColor 0.51 0.51 0.51\n\t\t-width 41\n\t\t-height 20\n\t\t-command \"refreshUVList;\"\n\t\t-commandRepeatable 1;\n\n\t\toptionMenu -w 76 -changeCommand \"polyUVSet -currentUVSet -uvSet #1\" -label \"\" uvSetDropDown;\n\t\tmenuItem -label $uvSet_All[0];\n\t\toptionMenu -edit -select 1 uvSetDropDown;\n\t\t//Add refresh menu items from current object\n\t\trefreshUVList();\n\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\ticonTextButton -style \"textOnly\"\n\t\t-labelOffset 30\n\t\t-label \"snapshot\"\n\t\t-backgroundColor 0.34 0.59 0.47\n\t\t-width 89\n\t\t-command \"button1maUVSnapShot;\"\n\t\t-commandRepeatable 1;\n\t\tseparator -width 1 -style \"none\";\n\t\t\n\ticonTextButton -style \"textOnly\"\n\t-labelOffset 30\n\t-label \"find\"\n\t-backgroundColor 0.51 0.51 0.51\n\t-width 27\n\t-command \"buttonFindUVSnapShot;\"\n\t-commandRepeatable 0;\n\t\n\tshowWindow;\n\tprint \"uvGrabber toolbox loaded\";\n}" 
        -sourceType "mel" 
        -commandRepeatable 1
        -flat 1
