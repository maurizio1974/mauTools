        -mi "Help weldTC" ( "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Help\n\nglobal proc m341_HelpWeldTo()\n{\n\tglobal string $m341_WeldTo_HelpWindow;\n\n\tif (`window -exists $m341_WeldTo_HelpWindow`)\n\t{\n\t\tglobal string $m341_WeldTo_HelpWindow;\n\t\tdeleteUI $m341_WeldTo_HelpWindow;\n\t}\n\t\n\t$m341_WeldTo_HelpWindow = `window -title \"Weld To Help\" -leftEdge 900 -topEdge 115 -toolbox true -sizeable false -resizeToFitChildren false -width 800 -height 210`;\n\tpaneLayout -configuration \"horizontal2\";\n\tscrollField -text \"weldTC/weldTT buttons:\\n\\nClick the weldTC button to weld currently selected vertices to the closest verts to them.\\n\\nSelect some vertices and click the weldTT button, then click on another vertex to target weld to it.\\n\\nAfter running either script once you can press the g key to repeat the process and save a lot of time.\\n\\nIf you select a component type other than vertex your selection will be converted to verts before the script runs. Sometimes it can be faster to select edges or faces especially when using the weldTT button.\\n\\nYoutube video here\\nhttps://youtu.be/kAOyQoMOGA4?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\"\n\t-wordWrap true\n\t-enableBackground true\n\t-backgroundColor 0.23 0.23 0.23;\n\t\n\tshowWindow $m341_WeldTo_HelpWindow;\n}\nm341_HelpWeldTo();" )
