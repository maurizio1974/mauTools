        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\nglobal string $m341_objExporterVersion;\n$m341_objExporterVersion = \"objHandler v1.0\";\n\n\n//*******************************************************************************************************\n//* global procs\n//*******************************************************************************************************\n//***********************************************************************************************************************************************************\n//Help\nglobal proc m341_objExporter_help()\n{\n\tglobal string $m341_objExporterVersion;\n\tglobal string $m341_objExporter_helpWindow;\n\n\tif (`window -exists $m341_objExporter_helpWindow`)\n\t{\n\t\tglobal string $m341_objExporter_helpWindow;\n\t\tdeleteUI $m341_objExporter_helpWindow;\n\t}\n\t\n\t$m341_objExporter_helpWindow = `window -title ($m341_objExporterVersion + \" Help\") -leftEdge 900 -topEdge 115 -toolbox true -sizeable false -resizeToFitChildren false -width 800 -height 800`;\n\t\t\n\tpaneLayout -configuration \"horizontal2\";\n\tscrollField -text \"RESET\\n-Resets the toolbox back to defualt settings.\\n\\n\\nFILE FOR EACH MESH CHECKBOX\\n-Turn on if you want to export a separate .obj file for each selected mesh when using the export obj button. File names are derived from the currently selected mesh names in the outliner.\\n\\n\\nTRIANGULATE CHECKBOX\\n-Turn on if you want the meshes in the exported .obj file to be triangulated. Meshes in the current scene are not affected.\\n\\n\\nCOMBINE MESHES CHECKBOX\\n-Turn on if you want the meshes in the exported .obj file to be combined into a single object. Meshes in the current scene are not affected.\\n\\n\\nMATERIALS CHECKBOX\\n-Turn on if you want materials to be included in the exported .obj file. Materials are stored in a separate file with the extension .mtl and that file is needed if you want to import the mesh with the correct materials applied to the mesh.\\n\\n\\nSMOOTH GROUPS CHECKBOX\\n-Turn on if you want smooting groups to be included in the exported .obj file.\\n\\n\\nNORMALS CHECKBOX\\n-Turn on if you want normals to be included in the exported .obj file.\\n\\n\\nEXPORT LOCATION FIELD\\n-The location the export obj button will export to. Copy paste a location or use the ... button to browse to a folder. You must select an existing folder, a file won't work.\\n\\n\\nEXPORT OBJ\\n-Exports selected objects as .obj to the folder specified in the export location field.\\n\\n\\nFIND\\n-Opens the folder specified in the export location field.\\n\\n\\nIMPORT LOCATION FIELD\\n-The location the import obj and all buttons will import from. Copy paste a location that includes a file name or use the ... button to broswe to a file. You must select an existing file, a folder won't work.\\n\\n\\nIMPORT OBJ\\n-Imports the .obj file specified in the import location field.\\n\\n\\nALL\\n-Imports all .obj files found in the folder specified in the import location field.\\n\\n\\nFIND\\n-Opens the folder specified in the import location field.\\n\\n\\nYOUTUBE VIDEO HERE\\nhttps://youtu.be/5ALeT_VyqKE?list=PLAUgGUDpaMENfz8Nd7Zw-LxDK_v5ZflY6\\n\\n\"\n\t-wordWrap true\n\t-enableBackground true\n\t-backgroundColor 0.23 0.23 0.23;\n\t\n\tshowWindow $m341_objExporter_helpWindow;\n}\n\n//*******************************************************************************************************\n//Reset \nglobal proc m341_objExporter_reset()\n{\n\t//text field\n\ttextField -e -text \"\" objExporter_exportLocation_TextFieldName;\n\tm341_objExporter_exportLocationValueChanged();\n\t\n\t//text field\n\ttextField -e -text \"\" objExporter_importLocation_TextFieldName;\n\tm341_objExporter_importLocationValueChanged();\n\t\n\t//File for each mesh\n\tcheckBox -e -value 0 objExporter_fileForEachMesh_CheckBoxName;\n\tglobal int $m341_objExporter_fileForEachMesh_userInput;\n\t$m341_objExporter_fileForEachMesh_userInput = `checkBox -q -value objExporter_fileForEachMesh_CheckBoxName`;\n\toptionVar -intValue \"m341_objExporter_fileForEachMeshCheckBox_VarName\" $m341_objExporter_fileForEachMesh_userInput;\t\n\t\n\t//Triangulate\n\tcheckBox -e -value 0 objExporter_triangulate_CheckBoxName;\n\tglobal int $m341_objExporter_triangulate_userInput;\n\t$m341_objExporter_triangulate_userInput = `checkBox -q -value objExporter_triangulate_CheckBoxName`;\n\toptionVar -intValue \"m341_objExporter_triangulateCheckBox_VarName\" $m341_objExporter_triangulate_userInput;\t\n\t\n\t//Combine meshes\n\tcheckBox -e -value 0 objExporter_combineMeshes_CheckBoxName;\n\tglobal int $m341_objExporter_combineMeshes_userInput;\n\t$m341_objExporter_combineMeshes_userInput = `checkBox -q -value objExporter_combineMeshes_CheckBoxName`;\n\toptionVar -intValue \"m341_objExporter_combineMeshesCheckBox_VarName\" $m341_objExporter_combineMeshes_userInput;\t\n\t\n\t//Materials\n\tcheckBox -e -value 1 objExporter_materials_CheckBoxName;\n\tglobal int $m341_objExporter_materials_userInput;\n\t$m341_objExporter_materials_userInput = `checkBox -q -value objExporter_materials_CheckBoxName`;\n\toptionVar -intValue \"m341_objExporter_materialsCheckBox_VarName\" $m341_objExporter_materials_userInput;\t\n\t\n\t//Smoothing\n\tcheckBox -e -value 1 objExporter_smoothing_CheckBoxName;\n\tglobal int $m341_objExporter_smoothing_userInput;\n\t$m341_objExporter_smoothing_userInput = `checkBox -q -value objExporter_smoothing_CheckBoxName`;\n\toptionVar -intValue \"m341_objExporter_smoothingCheckBox_VarName\" $m341_objExporter_smoothing_userInput;\n\t\n\t//Normals\n\tcheckBox -e -value 1 objExporter_normals_CheckBoxName;\n\tglobal int $m341_objExporter_normals_userInput;\n\t$m341_objExporter_normals_userInput = `checkBox -q -value objExporter_normals_CheckBoxName`;\n\toptionVar -intValue \"m341_objExporter_normalsCheckBox_VarName\" $m341_objExporter_normals_userInput;\t\n\t\n\tglobal string $m341_objExporterVersion;\n\tprint ($m341_objExporterVersion + \" reset to default\");\n}\n\n\n//***********************************************************************************************************************************************************\n//Export Obj\nglobal proc m341_objExporter_exportObj()\n{\n\t//Update text UI to get latest user typed\n\tglobal string $m341_objExporter_exportLocation_UserInput;\n\tm341_objExporter_exportLocationValueChanged();\n\n\t//Make folder if it doesn't exist\n\tsysFile -makeDir $m341_objExporter_exportLocation_UserInput;\n\n\t//Attach file name and .obj extension\n\tint $counterName = 0;\n\tstring $name[] = `ls -sl -fl`;\n\tstring $selectedObj[] = `ls -sl -fl`;\n\tstring $fileNameExtension = ($m341_objExporter_exportLocation_UserInput + \"\\\\\" + $name[$counterName] +\".obj\");\n\n\n\t//Triangulate\n\tglobal int $m341_objExporter_triangulate_userInput;\n\tstring $triangulated[] = `ls -sl`;\n\tif ($m341_objExporter_triangulate_userInput == 1)\n\t{\n\t\tpolyPerformAction polyTriangulate f 0;\n\t\ttoggleSelMode;\n\t\ttoggleSelMode;\n\t\tselectMode -object;\n\t}\n\tstring $historyNode[] = `listHistory $triangulated`;\n\t\n\t//Combine meshes\n\tglobal int $m341_objExporter_combineMeshes_userInput;\n\tglobal int $m341_objExporter_combineMeshes_ExpSetting;\n\t$m341_objExporter_combineMeshes_ExpSetting = 1;\n\tif ($m341_objExporter_combineMeshes_userInput == 1)\n\t{\n\t\tglobal int $m341_objExporter_combineMeshes_ExpSetting;\n\t\t$m341_objExporter_combineMeshes_ExpSetting = 0;\n\t}\n\t\n\t//Point groups always exported for now\n\tglobal int $m341_objExporter_pointGroups_userInput;\n\tglobal int $m341_objExporter_pointGroups_ExpSetting;\n\t$m341_objExporter_pointGroups_ExpSetting = 0;\n\tif ($m341_objExporter_pointGroups_userInput == 1)\n\t{\n\t\tglobal int $m341_objExporter_p