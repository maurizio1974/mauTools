n\t\t{\n\t\t\tselect -add $item;\n\t\t\tfloat $uvBoundingBox[] = `polyEvaluate -boundingBoxComponent2d`;\n\t\t\tfloat $widthU = $uvBoundingBox[1] - $uvBoundingBox[0];\n\t\t\tfloat $heightV = $uvBoundingBox[3] - $uvBoundingBox[2];\n\t\t\tif ($widthU == $widthCorner_U)\n\t\t\t{\n\t\t\t\t//Do nothing\n\t\t\t}\n\t\t\telse\n\t\t\t{\n\t\t\t\tselect -deselect $item;\n\t\t\t}\n\t\t}\n\t}\n}\n\nglobal proc m341_rectangleUVs()\n{\n\tundoInfo -openChunk;\n\t//Error\n\t$originalComponents = `ls -sl`;\n\t$originalComponentsSize = `size $originalComponents`;\n\tif ($originalComponentsSize == 0)\n\t{\n\t\terror \"Please select something first\";\n\t}\n\t\n\t//Error\n\t//string $errorFace[] = `filterExpand -ex 1 -sm 34`;\n\t//int $sizeErrorFace = `size $errorFace`;\n\t//if ($sizeErrorFace < 4)\n\t//{\n\t//\terror \"Please select four or more faces that form a square or rectangle shape\";\n\t//}\n\t//Error\t\n\t//else\n\t//{\n\t//\tstring $Sel[] = `filterExpand -ex 1 -sm 34`;\n\t//\tfloat $sizeSel = `size $Sel`;\n\t//\tfloat $sizeSel = $sizeSel /= 2;\n\t//\tstring $stringNumb = $sizeSel;\n\n\t//\tif(`gmatch $stringNumb \"*.5*\"`)\n\t//\t{\n\t//\t\terror \"Please select an equal number of faces that form a square or rectangle shape\";\n\t//\t}\t\t\n\t//}\n\n\t//No faces selected to start\n\tstring $uvGridObjMode[] = `filterExpand -ex 1 -sm 34`;\n\n\t//Gridify best guess axis\n\tstring $uvGridEdgeSel[] = `filterExpand -ex 1 -sm 32`;\n\tstring $uvGridFaceSel[] = `filterExpand -ex 1 -sm 34`;\n\n\t//Check if object mode selected and convert to all faces\n\tint $sizeUVGridObjMode = `size $uvGridObjMode`;\n\tif ($sizeUVGridObjMode == 0)\n\t{\n\t\tConvertSelectionToFaces;\n\t\tglobal string $m341_gridUVObjModeSelect[];\n\t\t$m341_gridUVObjModeSelect = `ls -sl`;\n\t}\n\t\n\tpolyForceUV -unitize;\n\tConvertSelectionToContainedEdges;\n\tConvertSelectionToEdges;\n\t$selectedEdges = `ls -sl`;\n\tselect -deselect $uvGridEdgeSel;\n\tpolyMapSewMove -nf 10 -lps 0 -ch 0;\n\tselect $selectedEdges;\n\t