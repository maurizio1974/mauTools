        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Face Y Axis v1.3\n\n//Unfreeze transform v1.2\nglobal proc m341_unfreezeTransform()\n{\n\ttoggleSelMode;\n\ttoggleSelMode;\n\tselectMode -object;\n\n\tstring $unfreeze_Selection[] = `ls -sl`;\n\tfor ($unfreeze_Item in $unfreeze_Selection)\n\t{\n    \t//Get rotate pivot info from object\n\t\tfloat $worldRotatePivot[];\n    \tmakeIdentity -apply true -t 1 -r 1 -s 1 -n 0 -pn 1 $unfreeze_Item;\n\t\tfloat $worldTranslatePivot[] = `getAttr ($unfreeze_Item + \".rotatePivot\")`;\n\t\tsetAttr ($unfreeze_Item + \".t\") (-1 * $worldTranslatePivot[0]) (-1 *$worldTranslatePivot[1]) (-1 *$worldTranslatePivot[2]);\n\n\t    //Paste back transform so it has correct transform from zero\n\t\tmakeIdentity -apply true -t 1 -r 1 -s 1 -n 0 -pn 1 $unfreeze_Item;\n\t\tsetAttr ($unfreeze_Item +\".t\") $worldTranslatePivot[0] $worldTranslatePivot[1] $worldTranslatePivot[2];\n\t\tsetAttr ($unfreeze_Item +\".r\") $worldRotatePivot[0] $worldRotatePivot[1] $worldRotatePivot[2];\n\t}\n}\n\n\nglobal proc m341_faceYup()\n{\n\t//Store as one undo\n\tundoInfo -openChunk;\n\n\tstring $component[] = `ls -fl -sl`;\n\ttoggleSelMode;\n\ttoggleSelMode;\n\tselectMode -object;\n\tstring $object[] = `ls -sl`;\n\tselect $component;\n\t\t\n\t//Store rebuilt rotation and translation in variable for later\n\t//Switch to move tool component mode\n\tstring $maCurrentMayaYear = `about -product`;\n\tif(`gmatch $maCurrentMayaYear \"*2017*\"`)\n\t{\n\t\tMoveTool;\n\t\tmanipMoveContext -edit -mode 9 Move;\n\t}\n\telse\n\t{\n\t\tMoveTool;\n\t\tmanipMoveContext -edit -mode 10 Move;\n\t}\n\t\n\t//Get move tool pivot position\n\tfloat $locatorTargetPOS[] = `manipMoveContext -q -position \"Move\"`;\n\t//Get move tool angle and convert radius to degrees\n\tfloat $locatorTargetAngle[] = `manipMoveContext -q -orientAxes \"Move\"`;\n\t$locatorTargetAngle[0] = rad_to_deg ($locatorTargetAngle[0]);\n\t$locatorTargetAngle[1] = rad_to_deg ($locatorTargetAngle[1]);\n\t$locatorTargetAngle[2] = rad_to_deg ($locatorTargetAngle[2]);\n\n    //Center pivot\n\tselect $object;\n\tCenterPivot;\n\n\t//Duplicate faces and delete all except one to avoid slow bake pivot bug\n\tduplicate;\n\tConvertSelectionToFaces;\n\tstring $componentsTooMany[] = `ls -sl -fl`;\n\tselect $componentsTooMany[0];\n\tInvertSelection;\n\tDelete;\n\tselect $componentsTooMany[0];\n\ttoggleSelMode;\n\ttoggleSelMode;\n\tselectMode -object;\n\t//Adjust rotate pivot so it aligns with average component selection from above\n\tmanipPivot -o $locatorTargetAngle[0] $locatorTargetAngle[1] $locatorTargetAngle[2];\n\tbakeCustomToolPivot 1 1;\n\tDeleteHistory;\t\n\tstring $rotatorObj[] = `ls -sl`;\n\t\t\n\t//Parent object under face with rebuilt transform\n\tparent $object $rotatorObj;\n\n\t//Unrotate parent face so child gets rotated back to zero correctly\n\tselect $rotatorObj;\n\trotate -os -fo 0 0 0;\n\n\t//Freeze transform of object and unparent\n\tselect $object;\n\tmakeIdentity -apply true -t 1 -r 1 -s 1 -n 0 -pn 1;\n\tparent -w;\n\n\t//Rotate group back to original angle and clean up\n\tdelete $rotatorObj;\n\t//Check if scene Z-Up and fix it with extra rotation\n\tstring $m341_upAxis = `upAxis -q -axis`;\n\tif(`gmatch $m341_upAxis \"*z*\"`)\n\t{\n\t\trotate -r -os -fo 0 -90 0; \n\t}\n\telse\n\t{\n\t\trotate -r -os -fo 0 0 90;\t\t\n\t}\n\tmakeIdentity -apply true -translate true -rotate true;\n\tm341_unfreezeTransform();\n\t\t\t\n   \tif(`gmatch $m341_upAxis \"*z*\"`)\n\t{\n\t    print \"Aligned to Z up\";\n\t}\n\telse\n\t{\n\t    print \"Aligned to Y up\";\n\t}\n\t\n\t//Undo above chunk\n    undoInfo -closeChunk;\n}\nm341_faceYup();\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n" 
