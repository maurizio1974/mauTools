        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Clean Duplicate Face v1.2\n\nglobal proc cleanConbineCleanGhostNodes()\n{\n\t//Store user selection\n\t$maCleanCombineAllSelected = `ls -sl`;\n\tDeleteHistory;\n\t{\n\t\t$Objs = `ls -sl`;\n\t\t$ObjsSize = `size $Objs`;\n\t\t$ObjCounter = 0;\n\t\t\n\t\tfor ($Item = 0; $Item < $ObjsSize; ++$Item)\n\t\t{\n\t\t\tselect $Objs[$ObjCounter];\n\t\t\tstring $shapeNode[] = `listRelatives -s`;\n\t\t\tint $shapeNodeSize = `size $shapeNode`;\n\t\t\tif ($shapeNodeSize > 1)\n\t\t\t{\n\t\t\t\tselect $shapeNode[0];\n\t\t\t\tpickWalk -d right;\n\t\t\t\tstring $ghostNode[] = `ls -sl`;\n\t\t\n\t\t\t\tif(`gmatch $ghostNode[0] \"*polySurfaceShape*\"`)\n\t\t\t\t{\n\t\t\t\t\t//If has no verts delete it\n\t\t\t\t\tConvertSelectionToVertices;\n\t\t\t\t\tstring $checkIfVerts[] = `filterExpand -sm 31`;\n\t\t\t\t\tint $checkIfVertsSize = `size $checkIfVerts`;\n\t\t\t\t\tif ($checkIfVertsSize == 0)\n\t\t\t\t\t{\n\t\t\t\t\t\tdelete;\n\t\t\t\t\t}\t\n\t\t\t\t}\n\t\t\t}\n\t\t\t$ObjCounter = $ObjCounter + 1;\n\t\t}\n\t}\n\t//Select user selection so above clean up doesn't break below script\n\tselect $maCleanCombineAllSelected;\n}\n\nglobal proc m341_cleanDuplicateFaces()\n{\n\t//Error if any xforms selected\n\tstring $checkForXformsSelected[] = `ls -sl -exactType transform`;\n\t$sizecheckForXformsSelected = `size $checkForXformsSelected`;\n\tif ($sizecheckForXformsSelected >= 1)\n\t{\n\t\terror \"Please select faces only\";\n\t}\n\t\n\tstring $selected_Faces[] = `ls -sl`;\n\ttoggleSelMode;\n\ttoggleSelMode;\n\tselectMode -object;\n\t//Delete ghost nodes if any found\n\tcleanConbineCleanGhostNodes();\n\tstring $errorSelectedObjs[] = `ls -sl`;\n\t\n\t//Error if faces selected on more than one object\n\tstring $countObjs[] = `ls -sl`;\n\tint $size_countObjs = `size $countObjs`;\n\tif ($size_countObjs > 1)\n\t{\n\t\tselect $selected_Faces;\n\t\tselect -add $errorSelectedObjs;\n\t\ttoggleSelMode;\n\t\terror \"Please select faces on one object only\";\n\t}\n\t\n\tstring $recordName_1[] = `ls -sl`;\n\t//Tokenize name to avoid group name getting pasted into renamed object if there was another object in the scene with the same name\t\n\tstring $newNameTokens[];\n\ttokenize $recordName_1[0] \"|\" $newNameTokens;\n\t$recordName_1[0] = $newNameTokens[size($newNameTokens)-1];\n\n\tselectMode -object;\n\t\n\tDeleteHistory;\n\t$selected_Obj_1 = `ls -sl`;\n\tduplicate -rr;\n\t$selected_Obj_2 = `ls -sl`;\n\t\n\tselect $selected_Obj_1;\n\tchangeSelectMode -component;\n\tselect $selected_Faces;\n\t\n\tInvertSelection;\n\tDelete;\n\tchangeSelectMode -object;\n\t\n\tselect $selected_Obj_1;\n\trename duplicated_01;\n\tCenterPivot;\n\tmakeIdentity -apply true -t 1 -r 1 -s 1 -n 0 -pn 1;\n\t\n\t$recordObj = `ls -sl`;\n\tselect $selected_Obj_2;\n\trename $recordName_1;\n\n\tselect $recordObj;\n\t//select -add m341_CleanDupeFace;\n\t\n\t//Check if object had children and run special clean up\n\t$childrenObj = `ls -sl`;\n\tstring $checkForChildren[] = `listRelatives -fullPath -type transform`;\n\t$size_checkForChildren = `size $checkForChildren`;\n\tif ($size_checkForChildren > 0)\n\t{\n\t\tselect $checkForChildren;\n\t\tdelete;\n\t\tselect $childrenObj;\n\t\tCenterPivot;\n\t\tmakeIdentity -apply true -t 1 -r 1 -s 1 -n 0 -pn 1;\n\t}\n\tprint \"Duplicated\";\n}\nm341_cleanDuplicateFaces();\n" 
