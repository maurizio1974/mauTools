`optionVar -q \"m341_uvMapper_pixPerUnitFloatField_VarName\"`;\n\t}\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\ttext -label \"  pix per unit \" -height 19;\n\tfloatField -value $uvMapper_pixPerUnit_floatFieldValue -pre 4 -width 56 -height 19 -changeCommand \"m341_uvMapper_pixPerUnit_ValueChanged();\" uvMapper_pixPerUnit_FloatFieldName;\n\n\t\n\t\n\t//global float $m341_query_TDValue;\n\t//global string $m341_uvMapper_pixPerUnitfloatFieldName;\n\t//global float $m341_uvMapper_pixPerUnit_UserInput;\n\t//setParent ..;\n\t//rowColumnLayout -numberOfRows 1;\n\t//text -label \"  pix per unit \" -height 19;\n\t//$m341_uvMapper_pixPerUnitfloatFieldName = `floatField -value 5.12 -pre 4 -width 56 -height 19 -changeCommand \"UVmapperTD_FloatValueChanged();\" PPU_FieldName`;\n\t\n\tsetParent ..;\n\trowColumnLayout -numberOfRows 1;\n\ticonTextButton -style \"textOnly\"\n\t\t-label \"copy TD\"\n\t\t-backgroundColor 0.70 0.39 0.34\n\t\t-width 62\n\t\t-command \"m341_uvMapper_copyTexelDensity;\"\n\t\t-commandRepeatable 1;\n\t\tseparator -width 1 -style \"none\";\n\t\t\n\ticonTextButton -style \"textOnly\"\n\t\t-label \"paste TD\"\n\t\t-backgroundColor 0.70 0.39 0.34\n\t\t-width 61\n\t\t-command \"m341_uvMapper_pasteTexelDensity;\"\n\t\t-commandRepeatable 1;\t\t\n\t\n\tshowWindow;\n}\n//***********************************************************************************************************************************************************\n//* Launch Window\n//***********************************************************************************************************************************************************\nif (`window -exists m341_uvMapper_toolboxName`)\n{\n\tdeleteUI m341_uvMapper_toolboxName;\n};\n\n{\n\tint $unfold3dPluginLoaded = `pluginInfo -q -loaded \"Unfold3D.mll\"`;\n\tif ($unfold3dPluginLoaded == 1)\n    {\n\t\tm341_launchUVMapperInterface();\n\t\tprint ($m341_uvMapperVersion + \" toolbox loaded\\n\");\n    }\n\n    else\n    {\n        loadPlugin \"Un