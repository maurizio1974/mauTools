        -command "//This is a paid script pack and is not free to use. Please purchase a copy and download the script pack here https://gumroad.com/malcolm341\n\n//Thanks very much for your support! Each purchase helps maintain the ad free Youtube channel found here https://www.youtube.com/malcolm341\n\n//Weld Verts to Target v1.2\n\n//Script job to move vert to target\nglobal proc m341_WeldAll_RunScriptJobProc()\n{\n    //Fix meters\n   \t//If Maya set to meters divide transform by 10,000\n\tstring $mayaUnits = `currentUnit -q`;\n\tif (`gmatch $mayaUnits \"m\"`)\n\t{\n\t\t//Get the position of the target vert\n\t    MoveTool;\n\t    float $mergeTargetPOS[] = `manipMoveContext -q -position \"Move\"`;\n   \t\t//Divide by 10,000 for meters\n\t\t$mergeTargetPOSMeter[0] = $mergeTargetPOS[0] /= 10000;\n\t\t$mergeTargetPOSMeter[1] = $mergeTargetPOS[1] /= 10000;\n\t\t$mergeTargetPOSMeter[2] = $mergeTargetPOS[2] /= 10000;\n\t    SelectTool;    \n    \t//Store the target vert name\n\t    string $targetVert[] = `ls -sl -fl`;\n    \n    \t//Select the merged verts and move them to the target\n\t    global string $vertToMove[];\n    \tselect $vertToMove;\n\t    move -absolute -worldSpace -worldSpaceDistance $mergeTargetPOS[0] $mergeTargetPOS[1] $mergeTargetPOS[2];\n\t    //Merge last two verts\n\t    select -add $targetVert;\n    \tMergeToCenter;\n\t}\n\telse\n\t{\n\t\t//Get the position of the target vert\n\t    MoveTool;\n    \tfloat $mergeTargetPOS[] = `manipMoveContext -q -position \"Move\"`;\n\t    SelectTool;    \n    \t//Store the target vert name\n\t    string $targetVert[] = `ls -sl -fl`;\n    \n    \t//Select the merged verts and move them to the target\n\t    global string $vertToMove[];\n    \tselect $vertToMove;\n\t    move -absolute -worldSpace -worldSpaceDistance $mergeTargetPOS[0] $mergeTargetPOS[1] $mergeTargetPOS[2];\n        //Merge last two verts\n\t    select -add $targetVert;\n    \tMergeToCenter;\n\t}    \n\n    //Convert to object mode and delete history\n    toggleSelMode;\n    toggleSelMode;\n    selectMode -object;\n    DeleteHistory;\n    \n   \tglobal int $m341_weldTT_component_Face;    \n    if ($m341_weldTT_component_Face > 0)\n\t{\n\t\tSelectFacetMask;\n\t}\n\n\tglobal int $m341_weldTT_component_Shell;\n\tif ($m341_weldTT_component_Shell > 0)\n\t{\n\t\tSelectMeshUVShell;\n\t}\n\n\tglobal int $m341_weldTT_component_Vert;\t\n\tif ($m341_weldTT_component_Vert > 0)\n\t{\n\t\tSelectVertexMask;\n\t}\n\n\tglobal int $m341_weldTT_component_Edge;\n\tif ($m341_weldTT_component_Edge > 0)\n\t{\n\t\tSelectEdgeMask;\n\t}\n\n\tglobal int $m341_weldTT_component_UV;\n\tif ($m341_weldTT_component_UV > 0)\n\t{\n\t\tSelectUVMask;\n\t}\n\n\tglobal int $m341_weldTT_component_Multi;\t\n\tif ($m341_weldTT_component_Multi > 0)\n\t{\n\t\tSelectMultiComponentMask;\n\t}\n    \n    SelectTool;    \n    print \"Weld to target complete\";\n}\n\nglobal proc m341_weldVertsToTarget()\n{\t\n  \t//Error if the selection mask is in object mode bacause it will take too long\n    string $xformCheck[] = `ls -sl -exactType transform`;\n    int $xformCheckSize = `size $xformCheck`;\n    if ($xformCheckSize > 0)\n    {\n        error \"Please select one or more components\";\n    }\n    \n    //Query what component mode the user is in\n\tglobal int $m341_weldTT_component_Face;\n\t$m341_weldTT_component_Face = `selectType -q -facet`;\n\t\n\tglobal int $m341_weldTT_component_Shell;\n\t$m341_weldTT_component_Shell = `selectType -q -meshUVShell`;\n\t\n\tglobal int $m341_weldTT_component_Vert;\n\t$m341_weldTT_component_Vert = `selectType -q -vertex`;\n\t\n\tglobal int $m341_weldTT_component_Edge;\n\t$m341_weldTT_component_Edge = `selectType -q -edge`;\n\t\n\tglobal int $m341_weldTT_component_UV;\n\t$m341_weldTT_component_UV = `selectType -q -polymeshUV`;\n\t\n\tglobal int $m341_weldTT_component_Multi;\n\t$m341_weldTT_component_Multi = `selectType -q -meshComponents`;\n    \n    \n    //Convert selection to verts and merge\n    ConvertSelectionToVertices;\n    SelectVertexMask;\n\n    global string $vertToMove[];\n    $vertToMove = `ls -sl -fl`;\n\n    //Turn on script job so the next selected vert runs the move to target\n    $m341_WeldAll_ScriptJob = `scriptJob -compressUndo 1 -runOnce 1 -e \"SelectionChanged\" \"m341_WeldAll_RunScriptJobProc\"`;\n\n    SelectTool;    \n    print \"Select a target vertex to weld to\";\n}\nm341_weldVertsToTarget()\n\n" 
        -sourceType "mel" 
        -commandRepeatable 1
